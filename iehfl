[32m+[m[32m        //ƒë√≥ng form v√† l∆∞u treeView data[m
[32m+[m[32m        #region FormMain_FormClosing[m
[32m+[m[32m            private void FormMain_FormClosing(object sender, FormClosingEventArgs e)[m
[32m+[m[32m            {[m
[32m+[m[32m                timerAutoSave.Enabled = false;[m
[32m+[m[32m                new FormLoad(passwordBytes, treeView, this, notifyIcon);//l∆∞u d·ªØ li·ªáu[m
[32m+[m[32m                FormFindAndReplace.Dispose();[m
[32m+[m[32m                //l∆∞u k√≠ch th∆∞·ªõc form[m
[32m+[m[32m                string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\";[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    File.WriteAllText(path + "WindowState.txt", WindowState.ToString());[m
[32m+[m[32m                    string[] str = new string[2] {Width.ToString(), Height.ToString() };[m
[32m+[m[32m                    File.WriteAllLines(path + "Size.txt", str);[m
[32m+[m[32m                }[m
[32m+[m[32m                catch (Exception E)[m
[32m+[m[32m                {[m
[32m+[m[32m                    notifyIcon.ShowBalloonTip(1000, "Writing ShowTime.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                }[m
[32m+[m[32m                //[m
[32m+[m[32m                Dispose();//dispose ƒë·ªÉ form login bi·∫øt ƒë·ªÉ out ch∆∞∆°ng tr√¨nh[m
[32m+[m[32m            }[m
[32m+[m[32m        #endregion[m
