[32m+[m	[32m{[m
[32m+[m[32m        //k√Ω t·ª± ƒë∆∞·ª£c bi·ªát d√πng ƒë·ªÉ t√°ch d·ªØ li·ªáu[m
[32m+[m[32m        private const string lockCode = "!2||-@#l";[m
[32m+[m[32m        private const string nodesCode = "!2||-@#ns";[m
[32m+[m[32m        private const string leftCode = "!2||-@#(";[m
[32m+[m		[32mprivate const string rightCode = ")#@-||2!";[m
[32m+[m[32m        private const string pathCode = "!2||-@#p";[m
[32m+[m[32m        private const string nameCode = "!2||-@#x";[m
[32m+[m[32m        private const string passCode = "!)}2#$&^( 11/07/1998 )@#$%^&($( GiangVan )%#$&*||-@#p";//d√πng ƒë·ªÉ nh·∫≠n bi·∫øt r·∫±ng ng∆∞·ªùi d√πng ƒë√£ nh·∫≠p ƒë√∫ng password hay ch∆∞a[m
[32m+[m[32m        //[m
[32m+[m[32m        private string path = Application.StartupPath + "\\data.g";[m
[32m+[m
[32m+[m[32m        private byte U = new byte();[m
[32m+[m		[32mprivate bool Done = false;//d√πng ƒë·ªÉ ƒë√≥ng form khi ƒë·ªçc ho·∫∑c l∆∞u d·ªØ li·ªáu xong[m
[32m+[m		[32mprivate TreeView treeView;[m
[32m+[m
[32m+[m
[32m+[m[32m        private byte[] password;[m
[32m+[m[32m        static private byte[] passwordBytes;[m
[32m+[m[32m        static private string dataString = null;[m
[32m+[m[32m        string savingString = "";[m
[32m+[m
[32m+[m[32m        internal bool password_is_wrong = new bool();[m
[32m+[m
[32m+[m[32m        private bool run_ShowComputer;[m
[32m+[m[32m        internal string info;[m
[32m+[m[32m        NotifyIcon notifyIcon;[m
