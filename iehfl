[32m+[m[32m        private bool CheckSpecialCharacter(string specialCharacter)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (!textBox1.Text.Contains(specialCharacter))[m
[32m+[m[32m                return false;[m
[32m+[m[32m            new FormNotification("Not allowed use: \n\"" + specialCharacter + "\"", false);[m
[32m+[m[32m            //b√¥i ƒëen ƒëo·∫°n chu·ªói ƒë·∫∑c bi·ªát ƒë√≥[m
[32m+[m[32m            textBox1.SelectionStart = textBox1.Text.IndexOf(specialCharacter);[m
[32m+[m[32m            textBox1.SelectionLength = specialCharacter.Length;[m
[32m+[m[32m            //click v√†o control textBoxContent[m
[32m+[m[32m            textBox1.ScrollToCaret();[m
[32m+[m[32m            textBox1.Select();[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void buttonOk_Click(object sender, EventArgs e)[m
[32m+[m		[32m{[m
[32m+[m			[32m#region Ki·ªÉm tra c√°c l·ªói c√∫ ph√°p.[m
[32m+[m			[32mif (textBox1.Text.Replace(" ", "") == "")[m
[32m+[m			[32m{[m
[32m+[m[32m                new FormNotification("Empty name!", false);[m
[32m+[m[32m                textBox1.Text = "";[m
[32m+[m[32m                textBox1.Select();[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m			[32mforeach (TreeNode i in root.Nodes)[m
[32m+[m			[32m{[m
[32m+[m[32m                if (textBox1.Text == i.Text && nameOfNode != textBox1.Text)[m
[32m+[m[32m                {[m
[32m+[m[32m                    new FormNotification("Namesake!", false);[m
[32m+[m[32m                    textBox1.SelectAll();[m
[32m+[m[32m                    textBox1.Select();[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            if (CheckSpecialCharacter(nodesCode) || CheckSpecialCharacter(pathCode) || CheckSpecialCharacter(leftCode) || CheckSpecialCharacter(lockCode) || CheckSpecialCharacter(nameCode) || CheckSpecialCharacter(rightCode))[m
[32m+[m[32m                return;[m
[32m+[m[32m            #endregion[m
[32m+[m[32m            Text = textBox1.Text;[m
[32m+[m			[32mbutton = true;[m
[32m+[m			[32mClose();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mprivate void buttonCancel_Click(object sender, EventArgs e)[m
[32m+[m		[32m{[m
[32m+[m			[32mClose();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m[32m        private void comboBox1_SelectedIndexChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (comboBox1.SelectedItem.ToString() == "Get date") textBox1.Text = DateTime.Now.ToShortDateString();[m
[32m+[m[32m            if (comboBox1.SelectedItem.ToString() == "Get time") textBox1.Text = DateTime.Now.ToLongTimeString();[m
[32m+[m[32m            if (comboBox1.SelectedItem.ToString() == "Get date time") textBox1.Text = DateTime.Now.ToLongTimeString() + " - " + DateTime.Now.DayOfWeek + ", " + DateTime.Now.ToLongDateString();[m
[32m+[m[32m            if (comboBox1.SelectedItem.ToString() == "Get day of week") textBox1.Text = DateTime.Now.DayOfWeek.ToString();[m
[32m+[m[32m            if (comboBox1.SelectedItem.ToString() == "Revert") textBox1.Text = nameOfNode;[m
[32m+[m[32m            textBox1.Select();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void textBox1_KeyDown(object sender, KeyEventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (e.Control && e.KeyCode == Keys.C)[m
[32m+[m[32m                if(textBox1.TextLength > 0) Clipboard.SetText(textBox1.Text);[m
[32m+[m[32m            if (e.Control && e.KeyCode == Keys.A)[m
[32m+[m[32m                textBox1.SelectAll();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
