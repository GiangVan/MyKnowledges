[32m+[m[32m        //MenuStrip[m
[32m+[m[32m        #region MenuStrip[m
[32m+[m[32m            //xem th√¥ng tin ph·∫ßn c·ª©ng m√°y t√≠nh[m
[32m+[m[32m            private void settingToolStripMenuItem_Click_1(object sender, EventArgs e)[m
[32m+[m[32m            {[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (info == null)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        Cursor = Cursors.AppStarting;[m
[32m+[m[32m                        info = ShowComputer.Show();[m
[32m+[m[32m                        Cursor = Cursors.Default;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else[m
[32m+[m[32m                    {[m
[32m+[m[32m                        textBoxContent.Text = info;[m
[32m+[m[32m                        UnSelectedNode();[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                catch (Exception E)[m
[32m+[m[32m                {[m
[32m+[m[32m                    textBoxContent.Text = "Can't read your hardware infomation:\r\n\r\n" + E.ToString();[m
[32m+[m[32m                    MessageBox.Show(E.ToString(), "ERROR", MessageBoxButtons.OK, MessageBoxIcon.Error);[m
[32m+[m[32m                    Cursor = Cursors.Default;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            //save[m
[32m+[m[32m            private void saveToolStripMenuItem1_Click(object sender, EventArgs e)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormLoad(passwordBytes, treeView, this, notifyIcon);//l∆∞u d·ªØ li·ªáu[m
[32m+[m[32m            }[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        //c√°c thao t√°c tr√™n TreeNodes[m
[32m+[m[32m        #region TreeNode[m
[32m+[m
[32m+[m[32m                //b·∫Øt s·ª± ki·ªán Click chu·ªôt v√†o m·ªôt TreeNode[m
[32m+[m[32m                #region treeView_NodeMouseClick[m
[32m+[m[32m                    private void treeView_NodeMouseClick(object sender, TreeNodeMouseClickEventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        //n·∫øu treeView.SelectedNode ·ªü tr·∫°ng th√°i unlock m√† chuy·ªÉn sang m·ªôt TreeNode kh√°c B√äN NGO√ÄI treeView.SelectedNode th√¨ lock n√≥ v·ªõi ki·ªÅu ki·ªán AutoLock == True[m
[32m+[m[32m                        if (checkBoxAutoLock.Checked && treeView.SelectedNode != null && e.Node.Name != "!2||-@#x" && e.Node != treeView.SelectedNode && !SearchANode(e.Node, treeView.SelectedNode))[m
[32m+[m[32m                            LockNodes(e.Node, TreeRoot);[m
[32m+[m[32m                        //g√°n TreeNode ƒë√£ ch·ªçn cho treeView.SelectedNode[m
[32m+[m[32m                        if (treeView.SelectedNode != null)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            treeView.SelectedNode.BackColor = Color.Gainsboro;[m
[32m+[m[32m                            if (treeView.SelectedNode != cutedNode) treeView.SelectedNode.ForeColor = Color.DimGray;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        treeView.SelectedNode = e.Node;[m
[32m+[m[32m                        treeView.SelectedNode.BackColor = SystemColors.Highlight;[m
[32m+[m[32m                        if (treeView.SelectedNode != cutedNode) treeView.SelectedNode.ForeColor = SystemColors.Window;[m
[32m+[m[32m                        //hi·ªán n·ªôi dung c·ªßa node.[m
[32m+[m[32m                        if (treeView.SelectedNode.Name == "!2||-@#x")[m
[32m+[m[32m                            textBoxContent.Text = "";[m
[32m+[m[32m                        else[m
[32m+[m[32m                            ShowTreeNodeContent(e.Node);[m
[32m+[m[32m                        //hi·ªán MenuStrip v√† s√©t SelectedNode khi click chu·ªôt ph·∫£i.[m
[32m+[m[32m                        if (e.Button == MouseButtons.Right)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            //n·∫øu l√† root Node[m
[32m+[m[32m                            if (treeView.SelectedNode.Name == "!2||-@#x")[m
[32m+[m[32m                                        {[m
[32m+[m[32m                                contextMenuStrip_TreeRoot.Show(MousePosition);[m
[32m+[m[32m                                if (cutedNode == null)[m
[32m+[m[32m                                    pasteToolStripMenuItem.Visible = false;[m
[32m+[m[32m                                else[m
[32m+[m[32m                                    pasteToolStripMenuItem.Visible = true;[m
[32m+[m[32m                            }[m
[32m+[m[32m                            //n·∫øu l√† tree Node[m
[32m+[m[32m                            else[m
[32m+[m[32m                            {[m
[32m+[m[32m                                //edit Content[m
[32m+[m[32m                                #region[m
[32m+[m[32m                                    editContentToolStripMenuItem.Enabled = false;[m
[32m+[m[32m                                    try[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        if (textBoxContent.Text == treeView.SelectedNode.Name.Substring(0, treeView.SelectedNode.Name.IndexOf(nodesCode))) editContentToolStripMenuItem.Enabled = true;[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                    catch { }[m
[32m+[m[32m                                    if (textBoxContent.Text == treeView.SelectedNode.Name) editContentToolStripMenuItem.Enabled = true;[m
[32m+[m[32m                                #endregion[m
[32m+[m[32m                                //lock / unlock[m
[32m+[m[32m                                #region[m
[32m+[m[32m                                    try[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        //n·∫øu TreeNode ƒë∆∞·ª£c ƒë∆∞·ª£c lock[m
[32m+[m[32m                                        if (treeView.SelectedNode.Name.Contains(lockCode) && !treeView.SelectedNode.Name.Contains(nodesCode))[m
[32m+[m[32m                                        {[m
[32m+[m[32m                                            locktoolStripMenuItem.Text = "Unlock                   (Alt + L)";[m
[32m+[m[32m                                            locktoolStripMenuItem.Image = Properties.Resources.padlock_unlock_icon__1_;[m
[32m+[m[32m                                        }[m
[32m+[m[32m                                        //[m
[32m+[m[32m                                        else[m
[32m+[m[32m                                        {[m
[32m+[m[32m                                            locktoolStripMenuItem.Text = "Lock                       (Alt + L)";[m
[32m+[m[32m                                            locktoolStripMenuItem.Image = Properties.Resources.padlock_lock_icon__1_;[m
[32m+[m[32m                                        }[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                    catch[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        locktoolStripMenuItem.Text = "Lock                       (Alt + L)";[m
[32m+[m[32m                                        locktoolStripMenuItem.Image = Properties.Resources.padlock_lock_icon__1_;[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                #endregion[m
[32m+[m[32m                                contextMenuStrip_TreeNode.Show(MousePosition);[m
[32m+[m[32m                                //Change password[m
[32m+[m[32m                                #region[m
[32m+[m[32m                                    //ch·ªâ cho ph√©p ƒë·ªïi m·∫≠t kh·∫©u khi TreeNode ƒë√£ ƒë∆∞·ª£c unlock[m
[32m+[m[32m                                    if(treeView.SelectedNode.Name.Contains(nodesCode)) changePasswordToolStripMenuItem.Enabled = true;[m
[32m+[m[32m                                    else changePasswordToolStripMenuItem.Enabled = false;[m
[32m+[m[32m                                #endregion[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void WhenChoosingANode()[m
[32m+[m[32m                    {[m
[32m+[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private bool SearchANode(TreeNode node, TreeNode root)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if(root.Nodes == null || root.Nodes.Count < 1)[m
[32m+[m[32m                            return false;[m
[32m+[m[32m                        foreach (TreeNode item in root.Nodes)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if(item == node) return true;[m
[32m+[m[32m                            else if(SearchANode(node, item)) return true;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        return false;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void LockNodes(TreeNode node, TreeNode root)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if(SearchANode(node, root))[m
[32m+[m[32m                        {[m
[32m+[m[32m                            foreach (TreeNode a_node in root.Nodes)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                if(node != a_node) LockNodes(node, a_node);[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        else[m
[32m+[m[32m                            KillNodes(root);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void KillNodes(TreeNode root)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if(root.Nodes == null && root.Name.Contains(nodesCode)) SaveAndLockTheUnlockedTreeNode(root);[m
[32m+[m[32m                        foreach (TreeNode a_node in root.Nodes)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            KillNodes(a_node);[m
[32m+[m[32m                        }[m
[32m+[m[32m                        if (root.Name.Contains(nodesCode))[m
[32m+[m[32m                            SaveAndLockTheUnlockedTreeNode(root);[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //c√°c thao t√°c tr√™n b√†n ph√≠m[m
[32m+[m[32m                #region[m
[32m+[m[32m                    private void treeView_KeyDown(object sender, KeyEventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        e.Handled = true;[m
[32m+[m[32m                        //di chuy·ªÉn l√™n top                                                         Page Up[m
[32m+[m[32m                        if (treeView.SelectedNode != null && e.KeyCode == Keys.PageUp && treeView.SelectedNode.Index != 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            TreeNode a = treeView.SelectedNode;[m
[32m+[m[32m                            TreeNode b = treeView.SelectedNode.Parent;[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            b.Nodes.Insert(0, a);[m
[32m+[m[32m                            treeView.SelectedNode = a;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //di chuy·ªÉn xu·ªëng bot                                                       Page Down[m
[32m+[m[32m                        if(treeView.SelectedNode != null && treeView.SelectedNode.Parent != null && e.KeyCode == Keys.PageDown && treeView.SelectedNode.Index != treeView.SelectedNode.Parent.Nodes.Count - 1)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            TreeNode a = treeView.SelectedNode;[m
[32m+[m[32m                            int index = treeView.SelectedNode.Parent.Nodes.Count;[m
[32m+[m[32m                            TreeNode b = treeView.SelectedNode.Parent;[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            b.Nodes.Insert(index, a);[m
[32m+[m[32m                            treeView.SelectedNode = a;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //di chuy·ªÉn l√™n                                                             Ctrl + Up[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.Up && treeView.SelectedNode != null && treeView.SelectedNode.Index != 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            TreeNode a = treeView.SelectedNode;[m
[32m+[m[32m                            int index = treeView.SelectedNode.Index - 1;[m
[32m+[m[32m                            TreeNode b = treeView.SelectedNode.Parent;[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            b.Nodes.Insert(index, a);[m
[32m+[m[32m                            treeView.SelectedNode = a;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //di chuy·ªÉn xu·ªëng                                                           Ctrl + Down[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.Down && treeView.SelectedNode != null && treeView.SelectedNode.Parent != null && treeView.SelectedNode.Index != treeView.SelectedNode.Parent.Nodes.Count - 1)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            TreeNode a = treeView.SelectedNode;[m
[32m+[m[32m                            int index = treeView.SelectedNode.Index + 1;[m
[32m+[m[32m                            TreeNode b = treeView.SelectedNode.Parent;[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            b.Nodes.Insert(index, a);[m
[32m+[m[32m                            treeView.SelectedNode = a;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //ch·ªânh s·ª≠a m·ªôt Tree node                                                   Ctrl + D or F3[m
[32m+[m[32m                        if (((e.Control && e.KeyCode == Keys.D) || e.KeyCode == Keys.F3) && treeView.SelectedNode != null && textBoxContent.ReadOnly)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            OpenEditContent_of_SelectedNode();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //t·∫°o m·ªõi m·ªôt Node                                                          Ctrl + N[m
[32m+[m[32m                        if(e.Control && e.KeyCode == Keys.N)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            CreateTreeNode();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //Rename                                                                    F2[m
[32m+[m[32m                        if(treeView.SelectedNode != null && e.KeyCode == Keys.F2)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            FormAdd formAdd = new FormAdd(MousePosition, treeView.SelectedNode.Parent, treeView.SelectedNode.Text);//ƒë·ªëi t∆∞·ª£ng tr·∫£ v·ªÅ n·ªôi dung ng∆∞·ªùi d√πng nh·∫≠p v√† ki·ªÉm tra, ngƒÉn nh·∫≠p t√™n gi·ªëng nhau trong c√πng m·ªôt nh√≥m node con c·ªßa node truy·ªÅn v√†o.[m
[32m+[m[32m                            if (formAdd.button)[m
[32m+[m[32m                                treeView.SelectedNode.Text = formAdd.Text;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //Delete                                                                    Delete[m
[32m+[m[32m                        if (treeView.SelectedNode != null && e.KeyCode == Keys.Delete && treeView.SelectedNode.Name != "!2||-@#x")[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (MessageBox.Show("Are you sure?", "Delete: " + treeView.SelectedNode.Text, MessageBoxButtons.YesNoCancel, MessageBoxIcon.Question, MessageBoxDefaultButton.Button3) == DialogResult.Yes)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                treeView.SelectedNode.Remove();[m
[32m+[m[32m                                textBoxContent.Text = "";[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //lock / Unlock                                                             Alt + L[m
[32m+[m[32m                        if(treeView.SelectedNode != null && e.Alt && e.KeyCode == Keys.L)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            LockOrUnlockNode();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //sao ch√©p t√™n c·ªßa node v√†o clipboard                                       Ctrl + 1[m
[32m+[m[32m                        if(e.Control && e.KeyCode == Keys.D1 && treeView.SelectedNode != null)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (treeView.SelectedNode.Text.Length > 0)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                if (treeView.SelectedNode.Text.Length > 0)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    Clipboard.SetText(treeView.SelectedNode.Text);[m
[32m+[m[32m                                    notifyIcon.ShowBalloonTip(1000, "Successful copy!", treeView.SelectedNode.Text, ToolTipIcon.None);[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //sao ch√©p n·ªôi dung c·ªßa node v√†o clipboard                                  Ctrl + 2[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.D2 && treeView.SelectedNode != null)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (treeView.SelectedNode.Name.Length > 0)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                if (treeView.SelectedNode.Name.Length > 0)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    Clipboard.SetText(treeView.SelectedNode.Name);[m
[32m+[m[32m                                    notifyIcon.ShowBalloonTip(1000, "Successful copy!", treeView.SelectedNode.Name, ToolTipIcon.None);[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //sao ch√©p to√†n b·ªô                                                          Ctrl + 3[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.D3 && treeView.SelectedNode != null)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (treeView.SelectedNode.Name.Length > 0 && treeView.SelectedNode.Text.Length > 0)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                string str = treeView.SelectedNode.Text + "\r\n\r\n" + treeView.SelectedNode.Name;[m
[32m+[m[32m                                if (str.Length > 0)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    Clipboard.SetText(str);[m
[32m+[m[32m                                    notifyIcon.ShowBalloonTip(1000, "Successful copy!", treeView.SelectedNode.Text + "\r\n\r\n" + treeView.SelectedNode.Name, ToolTipIcon.None);[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //t·∫°o m·ªõi m·ªôt TreeNode[m
[32m+[m[32m                #region newToolStripMenuItem_Click[m
[32m+[m[32m        private void newToolStripMenuItem_Click(object sender, EventArgs e)//t·∫°o m·ªõi m·ªôt n√∫t.[m
[32m+[m[32m                            {[m
[32m+[m[32m                                CreateTreeNode();[m
[32m+[m[32m                            }[m
[32m+[m[32m                            private void toolStripMenuItem3_Click(object sender, EventArgs e)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                CreateTreeNode();[m
[32m+[m[32m                            }[m
[32m+[m[32m                            private void CreateTreeNode()[m
[32m+[m[32m                            {[m
[32m+[m[32m                                FormAdd formAdd = new FormAdd(MousePosition, treeView.SelectedNode);//ƒë·ªëi t∆∞·ª£ng tr·∫£ v·ªÅ n·ªôi dung ng∆∞·ªùi d√πng nh·∫≠p v√† ki·ªÉm tra, ngƒÉn nh·∫≠p t√™n gi·ªëng nhau trong c√πng m·ªôt nh√≥m node con c·ªßa node truy·ªÅn v√†o.[m
[32m+[m[32m                                if (!formAdd.button) return;//n·∫øu ng∆∞·ªùi d√πng hu·ª∑ nh·∫≠p.[m
[32m+[m[32m                                TreeNode newNode = new TreeNode(formAdd.Text);[m
[32m+[m[32m                                treeView.SelectedNode.Nodes.Add(newNode);[m
[32m+[m[32m                                newNode.Parent.Expand();[m
[32m+[m[32m                                treeView.SelectedNode.BackColor = Color.Gainsboro;[m
[32m+[m[32m                                if (treeView.SelectedNode != cutedNode) treeView.SelectedNode.ForeColor = Color.DimGray;[m
[32m+[m[32m                                treeView.SelectedNode = newNode;[m
[32m+[m[32m                                treeView.SelectedNode.BackColor = SystemColors.Highlight;[m
[32m+[m[32m                                treeView.SelectedNode.ForeColor = SystemColors.Window;[m
[32m+[m[32m                                OpenEditting();[m
[32m+[m[32m                            }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //s·ª≠a t√™n m·ªôt TreeNode[m
[32m+[m[32m                #region editToolStripMenuItem_Click[m
[32m+[m[32m                    private void editToolStripMenuItem_Click(object sender, EventArgs e)//s·ª≠a t√™n m·ªôt n√∫t.[m
[32m+[m[32m                    {[m
[32m+[m[32m                        FormAdd formAdd = new FormAdd(MousePosition, treeView.SelectedNode.Parent, treeView.SelectedNode.Text);//ƒë·ªëi t∆∞·ª£ng tr·∫£ v·ªÅ n·ªôi dung ng∆∞·ªùi d√πng nh·∫≠p v√† ki·ªÉm tra, ngƒÉn nh·∫≠p t√™n gi·ªëng nhau trong c√πng m·ªôt nh√≥m node con c·ªßa node truy·ªÅn v√†o.[m
[32m+[m[32m                        if (!formAdd.button) return;//n·∫øu ng∆∞·ªùi d√πng hu·ª∑ nh·∫≠p.[m
[32m+[m[32m                        treeView.SelectedNode.Text = formAdd.Text;[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //s·ª≠a n·ªôi dung m·ªôt TreeNode[m
[32m+[m[32m                #region edit content of a TreeNode[m
[32m+[m[32m                    private void editContentToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        OpenEditting();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void OpenEditting()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        OpenEditContent_of_SelectedNode();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void CloseEditting()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        comboBoxSearch.Visible = true;[m
[32m+[m[32m                        panel_buttonSave.Visible = false;[m
[32m+[m[32m                        buttonCancel.Visible = false;[m
[32m+[m[32m                        treeView.Enabled = true;[m
[32m+[m[32m                        saveToolStripMenuItem1.Enabled = true;[m
[32m+[m[32m                        textBoxContent.ReadOnly = true;[m
[32m+[m[32m                        treeView.Select();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void buttonCancel_Click_1(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        CloseEditting();[m
[32m+[m[32m                        textBoxContent.Text = old_text;[m
[32m+[m[32m                        old_text = "";[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void buttonSave_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        SaveContent();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private bool CheckSpecialCharacter(string specialCharacter)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (!textBoxContent.Text.Contains(specialCharacter)) return false;[m
[32m+[m[32m                        new FormNotification("Not allowed use: \n\"" + specialCharacter + "\"", false);[m
[32m+[m[32m                        //b√¥i ƒëen ƒëo·∫°n chu·ªói ƒë·∫∑c bi·ªát ƒë√≥[m
[32m+[m[32m                        textBoxContent.SelectionStart = textBoxContent.Text.IndexOf(specialCharacter);[m
[32m+[m[32m                        textBoxContent.SelectionLength = specialCharacter.Length;[m
[32m+[m[32m                        //click v√†o control textBoxContent[m
[32m+[m[32m                        textBoxContent.ScrollToCaret();[m
[32m+[m[32m                        textBoxContent.Select();[m
[32m+[m[32m                        return true;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void ShowTreeNodeContent(TreeNode node)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        //n·∫øu l√† TreeNode th∆∞·ªùng.[m
[32m+[m[32m                        if (!node.Name.Contains(lockCode))[m
[32m+[m[32m                            textBoxContent.Text = node.Name;[m
[32m+[m[32m                        //n·∫øu l√† UnlockedTreeNode[m
[32m+[m[32m                        else if (node.Name.Contains(lockCode) && node.Name.Substring(0, node.Name.IndexOf(lockCode)).Contains(nodesCode))[m[41m [m
[32m+[m[32m                            textBoxContent.Text = node.Name.Substring(0, node.Name.IndexOf(nodesCode));[m
[32m+[m[32m                        //n·∫øu l√† LockedTreeNode[m
[32m+[m[32m                        else[m[41m                                [m
[32m+[m[32m                            textBoxContent.Text = "Locked";[m
[32m+[m[32m                        }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //xo√° m·ªôt TreeNode[m
[32m+[m[32m                #region deleteToolStripMenuItem_Click[m
[32m+[m[32m                    private void deleteToolStripMenuItem_Click(object sender, EventArgs e)//xo√° m·ªôt node.[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (treeView.SelectedNode.Name == "!2||-@#x") return;//ngƒÉn xo√° root.[m
[32m+[m[32m                        if (MessageBox.Show("Are you sure?", "Delete: " + treeView.SelectedNode.Text, MessageBoxButtons.YesNoCancel, MessageBoxIcon.Question, MessageBoxDefaultButton.Button3) == DialogResult.Yes)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            textBoxContent.Text = "";[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //sao ch√©p v√†o clipboard[m
[32m+[m[32m                #region copy to clipboard[m
[32m+[m[32m                    //sao ch√©p n·ªôi dung c·ªßa node v√†o clipboard[m
[32m+[m[32m                    private void copyContentToolStripMenuItem1_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (treeView.SelectedNode.Name.Length > 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                        if (treeView.SelectedNode.Name.Length > 0)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                Clipboard.SetText(treeView.SelectedNode.Name);[m
[32m+[m[32m                                notifyIcon.ShowBalloonTip(1000, "Successful copy!", treeView.SelectedNode.Name, ToolTipIcon.None);[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
