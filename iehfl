[32m+[m[32mÔªøusing System;[m
[32m+[m[32musing System.Text.RegularExpressions;[m
[32m+[m[32musing System.Windows.Forms;[m
[32m+[m
[32m+[m[32mnamespace YourExperience[m
[32m+[m[32m{[m
[32m+[m[32m    public partial class FormLogin : Form[m
[32m+[m[32m    {[m
[32m+[m[32m        private byte[] passwordBytes;[m
[32m+[m
[32m+[m[32m        public FormLogin()[m
[32m+[m[32m        {[m
[32m+[m[32m            InitializeComponent();[m
[32m+[m[32m            labelVersion.Text = "Version: " + Application.ProductVersion;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m        private void textBoxPassword_TextChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            Regex regex = new Regex("[^!-~]");[m
[32m+[m[32m            if (regex.Matches(textBoxPassword.Text).Count != 0)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormNotification("Special character detection: \"" + textBoxPassword.Text.Substring(textBoxPassword.Text.Length - 1, 1) + "\"", false);[m
[32m+[m[32m                textBoxPassword.Text = "";[m
[32m+[m[32m            }[m
[32m+[m[32m            passwordBytes = System.Text.Encoding.ASCII.GetBytes(textBoxPassword.Text);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void buttonLogin_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            //ki·ªÉm tra ƒë·ªô d√†i[m
[32m+[m[32m            if(passwordBytes == null || passwordBytes.Length < 8)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormNotification("Number of chars must be more than EIGHT chars", false);[m
[32m+[m[32m                textBoxPassword.Select();[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            //[m
[32m+[m[32m            Hide();[m
[32m+[m[32m            using(FormMain formMain = new FormMain(passwordBytes, this))//s·ª≠ d·ª•ng using ƒë·ªÉ hide ƒë∆∞·ª£c notifyion :D[m
[32m+[m[32m            {[m
[32m+[m[32m                if (formMain.IsDisposed)[m
[32m+[m[32m                {[m
[32m+[m[32m                    Application.Exit();[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            Show();[m
[32m+[m[32m            new FormNotification("Wrong password!", false);[m
[32m+[m[32m            textBoxPassword.Text = "";[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void buttonShow_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            textBoxPassword.UseSystemPasswordChar = !textBoxPassword.UseSystemPasswordChar;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void textBoxPassword_KeyDown(object sender, KeyEventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (e.Control && e.KeyCode == Keys.C)[m
[32m+[m[32m                if (textBoxPassword.TextLength > 0) Clipboard.SetText(textBoxPassword.Text);[m
[32m+[m[32m            if (e.Control && e.KeyCode == Keys.A)[m
[32m+[m[32m                textBoxPassword.SelectAll();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
