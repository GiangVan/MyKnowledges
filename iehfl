[32m+[m[32m                    private void rightToolStripMenuItem1_Click(object sender, EventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    treeView.RightToLeft = RightToLeft.Yes;[m
[32m+[m[32m                    toolStripMenuItem8.Image = null;[m
[32m+[m[32m                    toolStripMenuItem9.Image = Properties.Resources._checked;[m
[32m+[m[32m                    leftToolStripMenuItem1.Image = null;[m
[32m+[m[32m                    rightToolStripMenuItem1.Image = Properties.Resources._checked;[m
[32m+[m[32m                    treeView.ExpandAll();[m
[32m+[m[32m                }[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m                //lock / unlock[m
[32m+[m[32m                #region[m
[32m+[m[32m                    private void toolStripMenuItem11_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        /*[m
[32m+[m
[32m+[m[32m                        Khi ch∆∞∆°ng tr√¨nh ch·∫°y th√¨ thu·ªôc t√≠nh Tag c·ªßa t·∫•t c·∫£ c√°c LockedTreeNodes s·∫Ω ƒë∆∞·ª£c th√™m Databytes v√†o[m[41m [m
[32m+[m[32m                        Databytes ch·ª©a d·ªØ li·ªáu c·ªßa m·ªôt file LockedTreeNode ho√†n ch·ªânh[m
[32m+[m[32m                        Khi ch∆∞∆°ng tr√¨nh l∆∞u, c√°c UnlockedTreeNodes s·∫Ω ƒë∆∞·ª£c l∆∞u v√†o file ri√™ng c·ªßa n√≥ c√≤n Name s·∫Ω ƒë∆∞·ª£c ƒë·∫∑t b·∫±ng path + lockCode ƒë·ªÉ g·ªôp chu·ªói (t·ª©c l√∫c n√†y TreeNode.Name = path + lockCode)[m
[32m+[m[32m                        Khi ch∆∞∆°ng tr√¨nh l∆∞u, c√°c LockedTreeNodes.Name s·∫Ω ƒë∆∞·ª£c ƒë·∫∑t b·∫±ng path + lockCode ƒë·ªÉ g·ªôp chu·ªói (t·ª©c l√∫c n√†y TreeNode.Name = path + lockCode)[m
[32m+[m[32m                        Tr·∫°ng th√°i c·ªßa Locked: TreeNode.Name = path + lockCode[m
[32m+[m[32m                        Tr·∫°ng th√°i c·ªßa Unlocked: TreeNode.Name = text-content + nodesCode + text-nodes + pathCode + path + lockCode + password[m
[32m+[m[32m                        M·ªôt TreeNode file s·∫Ω l∆∞u: passCode + text-content + nodesCode + text-nodes[m
[32m+[m
[32m+[m[32m                        */[m
[32m+[m[32m                        LockOrUnlockNode();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void LockOrUnlockNode()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        string path;[m
[32m+[m[32m                        byte[] bytes;[m
[32m+[m[32m                        //Lock[m
[32m+[m[32m                        #region[m
[32m+[m[32m                        if (treeView.SelectedNode.Name.Contains(nodesCode) || !treeView.SelectedNode.Name.Contains(lockCode))[m
[32m+[m[32m                        {[m
[32m+[m[32m                            //n·∫øu TreeNode ƒë√£ ƒë∆∞·ª£c unlock: th√¨ lock v√† l∆∞u[m
[32m+[m[32m                            if (treeView.SelectedNode.Name.Contains(lockCode))[m
[32m+[m[32m                            {[m
[32m+[m[32m                                SaveAndLockTheUnlockedTreeNode(treeView.SelectedNode);[m
[32m+[m[32m                                textBoxContent.Text = "Locked";[m
[32m+[m[32m                            }[m
[32m+[m[32m                            //n·∫øu TreeNode ch∆∞a ƒë∆∞·ª£c ƒë·∫∑t lock: th√¨ ƒë·∫∑t lock v√† l∆∞u n√≥ l·∫°i[m
[32m+[m[32m                            else[m
[32m+[m[32m                            {[m
[32m+[m[32m                                //t·∫°o m·ªôt m·∫≠t kh·∫©u m·ªõi[m
[32m+[m[32m                                using (FormPassword formPassword = new FormPassword(new Point(MousePosition.X - 12, MousePosition.Y - 50), "Set a new password"))[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    if (formPassword.GetPassword() == null)[m
[32m+[m[32m                                        return;//n·∫øu ng∆∞·ªùi d√πng hu·ª∑ vi·ªác t·∫°o m·∫≠t kh·∫©u th√¨ out[m
[32m+[m[32m                                    UseWaitCursor = true;[m
[32m+[m[32m                                    //chuy·ªÉn c√°c TreeNodes con c·ªßa TreeNode ƒë√≥ th√†nh chu·ªói[m[41m		[m
[32m+[m[32m                                    text_nodes = "";[m
[32m+[m[32m                                    TreeNodeToString(treeView.SelectedNode);[m
[32m+[m[32m                                    text_nodes += rightCode;[m
[32m+[m[32m                                    //t·∫°o th∆∞ m·ª•c ƒë·ªÉ l∆∞u file (n·∫øu ch∆∞a c√≥)[m
[32m+[m[32m                                    path = Application.StartupPath + "\\Data\\";[m
[32m+[m[32m                                    if (!Directory.Exists(path))[m
[32m+[m[32m                                        Directory.CreateDirectory(path);[m
[32m+[m[32m                                    //t·∫°o t√™n file[m
[32m+[m[32m                                    string fileName = DateTime.Now.Hour + "-" + DateTime.Now.Minute + "-" + DateTime.Now.Second + "-" + MousePosition.X.ToString() + "-" + MousePosition.Y.ToString() + "-" + treeView.SelectedNode.GetHashCode().ToString() + GetHashCode().ToString() + random.Next(-2147483648, 2147483647).ToString() + DateTime.Now.Ticks.ToString().Substring(12) + "-" + DateTime.Now.Day + "-" + DateTime.Now.Month + "-" + DateTime.Now.Year + ".g";[m
[32m+[m[32m                                    //t·∫°o bytes d·ªØ li·ªáu c·ªßa RI√äNG TreeNode ƒë√≥[m
[32m+[m[32m                                    bytes = GCoding.Encoding(Encoding.Unicode.GetBytes((passCode + treeView.SelectedNode.Name + nodesCode + text_nodes).ToCharArray()), formPassword.GetPassword());[m
[32m+[m[32m                                    //ti·∫øn h√†nh l∆∞u[m
[32m+[m[32m                                    WriteTheFile:[m
[32m+[m[32m                                    try[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        File.WriteAllBytes(path + fileName, bytes);[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                    catch (Exception E)[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        textBoxContent.Text = "Can't write the file:\t" + path + fileName + "\r\n\r\n" + E.ToString();[m
[32m+[m[32m                                        if (MessageBox.Show("Can't write the file: " + fileName + "\n" + E.ToString(), "ERROR", MessageBoxButtons.RetryCancel, MessageBoxIcon.Error) == DialogResult.Retry)[m
[32m+[m[32m                                            goto WriteTheFile;[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                    //c·∫≠p nh·∫≠t l·∫°i TreeNode v√† xo√° c√°c TreeNodes con c·ªßa n√≥[m
[32m+[m[32m                                    textBoxContent.Text = "Locked successfully!";[m
[32m+[m[32m                                    treeView.SelectedNode.Name = fileName + lockCode;[m
[32m+[m[32m                                    treeView.SelectedNode.Tag = new DataBytes(bytes);[m
[32m+[m[32m                                    treeView.SelectedNode.Nodes.Clear();[m
[32m+[m[32m                                    UseWaitCursor = false;[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                            return;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        #endregion[m
[32m+[m[32m                        //Unlock[m
[32m+[m[32m                        #region[m[41m [m
[32m+[m[32m                        UseWaitCursor = true;[m
[32m+[m[32m                        //m·ªü form ƒëƒÉng nh·∫≠p[m
[32m+[m[32m                        using (FormLog formLog = new FormLog("Unlock"))[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (formLog.GetPass() != null && formLog.is_Enter)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                //l·∫•y DataBytes[m
[32m+[m[32m                                if (treeView.SelectedNode.Tag == null)//tr∆∞·ªùng h·ª£p kh√¥ng t√¨m th·∫•y DataBytes (c√≥ nghƒ©a l√† vi·ªác ƒë·ªçc coded-text l√∫c kh·ªüi ƒë·ªông ch∆∞∆°ng tr√¨nh th·∫•t b·∫°i) th√¨ ƒë·ªçc l·∫°i[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    //t·∫°o th∆∞ m·ª•c ƒë·ªÉ l∆∞u file (n·∫øu ch∆∞a c√≥)[m
[32m+[m[32m                                    path = Application.StartupPath + "\\Data\\";[m
[32m+[m[32m                                    if (!Directory.Exists(path))[m
[32m+[m[32m                                        Directory.CreateDirectory(path);[m
[32m+[m[32m                                    string fileName = treeView.SelectedNode.Name.Substring(0, treeView.SelectedNode.Name.IndexOf(lockCode));[m
[32m+[m[32m                                    try[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        treeView.SelectedNode.Tag = new DataBytes(File.ReadAllBytes(path + fileName));[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                    catch (Exception E)[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        textBoxContent.Text = "Can't write the file:\t" + path + fileName + "\r\n\r\n" + E.ToString();[m
[32m+[m[32m                                        MessageBox.Show("Can't write the file: " + fileName + "\n" + E.ToString(), "ERROR", MessageBoxButtons.OKCancel, MessageBoxIcon.Error);[m
[32m+[m[32m                                        UseWaitCursor = false;[m
[32m+[m[32m                                        return;[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                }[m
[32m+[m[32m                                bytes = ((DataBytes)treeView.SelectedNode.Tag).bytes;[m
[32m+[m[32m                                //gi·∫£i m√£[m
[32m+[m[32m                                string data = Encoding.Unicode.GetString(GCoding.Decoding(bytes, formLog.GetPass()));[m
[32m+[m[32m                                if (data.Contains(passCode))//n·∫øu nh·∫≠p ƒë√∫ng password[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    data = data.Substring(passCode.Length);//data l√∫c n√†y ch·ª©a: text-content + nodesCode + text_nodes[m
[32m+[m[32m                                    text_nodes = data.Substring(data.IndexOf(nodesCode) + nodesCode.Length);[m
[32m+[m[32m                                    //N·∫°p c√°c TreeNodes cho TreeNode ƒë√≥[m
[32m+[m[32m                                    int index = -leftCode.Length;[m
[32m+[m[32m                                    while (true)[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        index += leftCode.Length;[m
[32m+[m[32m                                        if (index == text_nodes.Length - 1 || text_nodes.Substring(index).IndexOf(rightCode) == 0)[m
[32m+[m[32m                                            break;[m
[32m+[m[32m                                        index = AddTreeNode(treeView.SelectedNode, index);//ƒë·ªá quy[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                    treeView.SelectedNode.Expand();[m
[32m+[m[32m                                    //hi·ªÉn th·ªã text-content c·ªßa TreeNode[m
[32m+[m[32m                                    textBoxContent.Text = data.Substring(0, data.IndexOf(nodesCode));[m
[32m+[m[32m                                    //ƒë·∫∑t l·∫°i thu·ªôc t√≠nh Name cho TreeNode[m
[32m+[m[32m                                    treeView.SelectedNode.Name = data + pathCode + treeView.SelectedNode.Name.Substring(0, treeView.SelectedNode.Name.IndexOf(lockCode) + lockCode.Length) + Encoding.Default.GetString(formLog.GetPass());[m
[32m+[m[32m                                    treeView.SelectedNode.Tag = null;[m
[32m+[m[32m                                }[m
[32m+[m[32m                                else[m
[32m+[m[32m                                    new FormNotification("Wrong password!", false);[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        UseWaitCursor = false;[m
[32m+[m[32m                        #endregion[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //ƒë·ªá quy[m
[32m+[m[32m                    #region[m
[32m+[m[32m                    //L·∫•y c√°c TreeNodes v√† n·ªëi th√†nh chu·ªói string[m
[32m+[m[32m                    private void TreeNodeToString(TreeNode treeRoot)[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        if (treeRoot.Nodes == null) return;[m
[32m+[m[32m                                        foreach (TreeNode nodes in treeRoot.Nodes)[m
[32m+[m[32m                                        {[m
[32m+[m[32m                                            text_nodes += nodes.Text + nameCode + nodes.Name + leftCode;[m
[32m+[m[32m                                            TreeNodeToString(nodes);[m
[32m+[m[32m                                            text_nodes += rightCode;[m
[32m+[m[32m                                        }[m
[32m+[m[32m                                    }[m
[32m+[m[32m                                    //L·∫•y c√°c TreeNodes t·ª´ chu·ªói string[m
[32m+[m[32m                                    private int AddTreeNode(TreeNode treeRoot, int indexCurrent)[m
[32m+[m[32m                                    {[m
[32m+[m[32m                                        int index = text_nodes.Substring(indexCurrent).IndexOf(leftCode) + indexCurrent;[m
[32m+[m[32m                                        //t·∫°o m·ªõi m·ªôt TreeNode[m
[32m+[m[32m                                        TreeNode treeNode = new TreeNode();[m
[32m+[m[32m                                        string dataString2 = text_nodes.Substring(indexCurrent, index - indexCurrent);//ƒëo·∫°n string ch·ª©a d·ªØ li·ªáu c·ªßa m·ªôt TreeNode[m
[32m+[m[32m                                        indexCurrent = dataString2.IndexOf(nameCode);[m
[32m+[m[32m                                        treeNode.Text = dataString2.Substring(0, indexCurrent);//ƒëo·∫°n ƒë·∫ßu c·ªßa dataString2 ch·ª©a t√™n c·ªßa TreeNode[m
[32m+[m[32m                                        treeNode.Name = dataString2.Substring(indexCurrent + nameCode.Length);//ƒëo·∫°n c√≤n l·∫°i c·ªßa dataString2 ch·ª©a n·ªôi dung c·ªßa TreeNode[m
[32m+[m[32m                                        treeRoot.Nodes.Add(treeNode);[m
[32m+[m[32m                                        //b·∫Øt ƒë·∫ßu ƒë·ªá quy[m
[32m+[m[32m                                        while (true)[m
[32m+[m[32m                                        {[m
[32m+[m[32m                                            index += leftCode.Length;[m
[32m+[m[32m                                            if (index == text_nodes.Length - 1 || text_nodes.Substring(index).IndexOf(rightCode) == 0) return index;[m
[32m+[m[32m                                            index = AddTreeNode(treeNode, index);[m
[32m+[m[32m                                        }[m
[32m+[m[32m                                    }[m
[32m+[m[32m                    #endregion[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //change password[m
[32m+[m[32m                #region[m
[32m+[m[32m                    private void changePasswordToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        using (FormPassword formPassword = new FormPassword(new Point(MousePosition.X - 12, MousePosition.Y - 50), "Change password"))[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (formPassword.GetPassword() == null)[m
[32m+[m[32m                                return;//n·∫øu ng∆∞·ªùi d√πng hu·ª∑ vi·ªác t·∫°o m·∫≠t kh·∫©u th√¨ out[m
[32m+[m[32m                            treeView.SelectedNode.Name = treeView.SelectedNode.Name.Substring(0, treeView.SelectedNode.Name.IndexOf(lockCode) + lockCode.Length) + Encoding.Default.GetString(formPassword.GetPassword());[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        //panelMid[m
[32m+[m[32m        #region panelMid[m
[32m+[m[32m        private void panelMid_MouseDown(object sender, MouseEventArgs e)[m
[32m+[m[32m            {[m
[32m+[m[32m                oldLeft_of_panelMid = panelMid.Left;[m
[32m+[m[32m                left_of_mouse_and_panelMid = MousePosition.X - panelMid.Left;[m
[32m+[m[32m                clicked = true;[m
[32m+[m[32m            }[m
[32m+[m[32m            private void panelMid_MouseUp(object sender, MouseEventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            clicked = false;[m
[32m+[m[32m        }[m
[32m+[m[32m            //change location panelMid[m
[32m+[m[32m            private void panelMid_MouseMove(object sender, MouseEventArgs e)[m
[32m+[m[32m            {[m
[32m+[m[32m                if (!clicked || panelMid.Left < 35) return;[m
[32m+[m[32m                if (MousePosition.X - left_of_mouse_and_panelMid < 35) panelMid.Left = 35;[m
[32m+[m[32m                else if (Width - MousePosition.X + left_of_mouse_and_panelMid < 35) panelMid.Left = Width - 35;[m
[32m+[m[32m                else panelMid.Left = MousePosition.X - left_of_mouse_and_panelMid;[m
[32m+[m[32m                int index = oldLeft_of_panelMid - panelMid.Left;[m
[32m+[m[32m                panel_treeView.Left -= index;[m
[32m+[m[32m                panel_treeView.Width += index;[m
[32m+[m[32m                panelMain.Width -= index;[m
[32m+[m[32m                oldLeft_of_panelMid = panelMid.Left;[m
[32m+[m[32m            }[m
[32m+[m[32m            //Change size form[m
[32m+[m[32m            private void FormMain_SizeChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            //form ƒëang thu nh·ªè[m
[32m+[m[32m            if ((panelMid.Left - oldLeft_of_panelMid2) < 0 && panelMid.Left < 35 && WindowState != FormWindowState.Minimized)[m
[32m+[m[32m            {[m
[32m+[m[32m                panelMid.Anchor = AnchorStyles.Top | AnchorStyles.Bottom | AnchorStyles.Left;[m
[32m+[m[32m                panel_treeView.Anchor = AnchorStyles.Top | AnchorStyles.Bottom | AnchorStyles.Left | AnchorStyles.Right;[m
[32m+[m[32m                panelMain.Anchor = AnchorStyles.Top | AnchorStyles.Bottom | AnchorStyles.Left;[m
[32m+[m[32m                panelMid.Left = 35;[m
[32m+[m[32m                panel_treeView.Left = 46;[m
[32m+[m[32m                panel_treeView.Width = Width - panel_treeView.Left - 18;[m
[32m+[m[32m                panelMain.Width = 12;[m
[32m+[m[32m            }[m
[32m+[m[32m            //form ƒëang n·ªü r·ªông[m
[32m+[m[32m            else if (panelMid.Anchor == (AnchorStyles.Top | AnchorStyles.Bottom | AnchorStyles.Left) && panel_treeView.Width > 245)[m
[32m+[m[32m            {[m
[32m+[m[32m                panelMid.Anchor = AnchorStyles.Top | AnchorStyles.Bottom | AnchorStyles.Right;[m
[32m+[m[32m                panel_treeView.Anchor = AnchorStyles.Top | AnchorStyles.Bottom | AnchorStyles.Right;[m
[32m+[m[32m                panelMain.Anchor = AnchorStyles.Top | AnchorStyles.Bottom | AnchorStyles.Left | AnchorStyles.Right;[m
[32m+[m[32m            }[m
[32m+[m[32m            oldLeft_of_panelMid2 = panelMid.Left;[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        //comboBoxSearch[m
[32m+[m[32m        #region comboBoxSearch[m
[32m+[m[32m            private void comboBoxSearch_KeyDown(object sender, KeyEventArgs e)[m
[32m+[m[32m            {[m
[32m+[m[32m                if (e.KeyCode == Keys.Enter)[m
[32m+[m[32m                {[m
[32m+[m[32m                    Command();[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            private void Command()[m
[32m+[m[32m        {[m
[32m+[m[32m            if (comboBoxSearch.Text == "opencurrentfolder")[m
[32m+[m[32m            {[m
[32m+[m[32m                Process.Start(Directory.GetCurrentDirectory());[m
[32m+[m[32m                comboBoxSearch.Text = "";[m
[32m+[m[32m            }[m
[32m+[m[32m            else if (comboBoxSearch.Text == "openspecialfolder")[m
[32m+[m[32m            {[m
[32m+[m[32m                string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge";[m
[32m+[m[32m                if (Directory.Exists(path))[m
[32m+[m[32m                    Process.Start(path);[m
[32m+[m[32m                else[m
[32m+[m[32m                    MessageBox.Show("The path: \"" + path + "\" does not exist");[m
[32m+[m[32m                comboBoxSearch.Text = "";[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        //C√†i ƒë·∫∑t[m
[32m+[m[32m        #region Setting[m
[32m+[m
[32m+[m[32m            //ƒë√≥ng m·ªü panelSetting[m
[32m+[m[32m            #region[m
[32m+[m[32m                private void settingToolStripMenuItem1_Click(object sender, EventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    //if (!panelSetting.Enabled) return;[m
[32m+[m[32m                    //ƒë√≥ng c√†i ƒë·∫∑t[m
[32m+[m[32m                    if(panelSetting_is_open)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        panelSetting_is_open = false;[m
[32m+[m[32m                        if (checkBox_TurboBoost.Checked)[m
[32m+[m[32m                            panelSetting.Left = -210;[m
[32m+[m[32m                        else[m
[32m+[m[32m                            new AnimationMOVE(panelSetting, -210, false);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //m·ªü c√†i ƒë·∫∑t[m
[32m+[m[32m                    else[m
[32m+[m[32m                    {[m
[32m+[m[32m                        panelSetting_is_open = true;[m
[32m+[m[32m                        if (checkBox_TurboBoost.Checked)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            panelSetting.Left = 0;[m
[32m+[m[32m                            buttonFocus.Select();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        else[m
[32m+[m[32m                            new AnimationMOVE(panelSetting, 0, true);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            #endregion[m
[32m+[m[32m            //ƒë√≥ng panelSetting khi b·ªã outfocus[m
[32m+[m[32m            #region[m
[32m+[m[32m                private void panelSetting_Leave(object sender, EventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (panelSetting.Left == 0 && panelSetting.Enabled)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        //ƒë√≥ng c√†i ƒë·∫∑t[m
[32m+[m[32m                        panelSetting_is_open = false;[m
[32m+[m[32m                        if (checkBox_TurboBoost.Checked)[m
[32m+[m[32m                            panelSetting.Left = -210;[m
[32m+[m[32m                        else[m
[32m+[m[32m                            new AnimationMOVE(panelSetting, -210, false);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                private void panelSetting_LocationChanged(object sender, EventArgs e)[m
[32m+[m[32m            {[m
[32m+[m[32m                if (panelSetting.Left == 0) buttonFocus.Select();[m
[32m+[m[32m            }[m
[32m+[m[32m            #endregion[m
[32m+[m[32m            //Change password[m
[32m+[m[32m            #region[m
[32m+[m[32m                private void buttonChangePassword_Click(object sender, EventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    CheckPassword();[m
[32m+[m[32m                }[m
[32m+[m[32m                private void textBoxPassword_KeyDown(object sender, KeyEventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (e.KeyCode == Keys.Enter) CheckPassword();[m
[32m+[m[32m                    if (e.Control && e.KeyCode == Keys.C)[m
[32m+[m[32m                        if (textBoxPassword.TextLength > 0) Clipboard.SetText(textBoxPassword.Text);[m
[32m+[m[32m                    if (e.Control && e.KeyCode == Keys.A)[m
[32m+[m[32m                        textBoxPassword.SelectAll();[m
[32m+[m[32m                }[m
[32m+[m[32m                private void CheckPassword()[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (textBoxPassword.Text.Length == 0)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        new FormNotification("Empty password!", false);[m
[32m+[m[32m                        return;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //nh·∫≠p ƒë√∫ng m·∫≠t kh·∫©u[m
[32m+[m[32m                    if (Encoding.ASCII.GetString(passwordBytes) == textBoxPassword.Text)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        FormPassword formPassword = new FormPassword(new Point(Left + 30, Top + 350), "Enter a new password");[m
[32m+[m[32m                        if (formPassword.GetPassword() != null)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            passwordBytes = formPassword.GetPassword();[m
[32m+[m[32m                            new FormLoad(passwordBytes, treeView, this, notifyIcon);//l∆∞u d·ªØ li·ªáu[m
[32m+[m[32m                            notifyIcon.ShowBalloonTip(1000, "Change password successfully", " ", ToolTipIcon.None);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //nh·∫≠p sai m·∫≠t kh·∫©u[m
[32m+[m[32m                    else[m
[32m+[m[32m                        new FormNotification("Wrong password!", false);[m
[32m+[m[32m                    textBoxPassword.Text = "";[m
[32m+[m[32m                }[m
[32m+[m[32m            #endregion[m
[32m+[m[32m            //CheckBoxs[m
[32m+[m[32m            #region[m
[32m+[m[32m                //checkBox_StartWithWindows[m
[32m+[m[32m                private void checkBox_StartWithWindows_CheckedChanged(object sender, EventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    RegistryKey registryKey = Registry.CurrentUser.OpenSubKey("SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Run", true);[m
[32m+[m[32m                    if (checkBox_StartWithWindows.Checked)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (registryKey.GetValue("My Knowledge") == null)[m
[32m+[m[32m                            registryKey.SetValue("My Knowledge", Application.ExecutablePath);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (registryKey.GetValue("My Knowledge") != null)[m
[32m+[m[32m                            registryKey.DeleteValue("My Knowledge");[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\StartWithWindows.txt";[m
[32m+[m[32m                    try[m
[32m+[m[32m                    {[m
[32m+[m[32m                        File.WriteAllText(path, checkBox_StartWithWindows.Checked.ToString());[m
[32m+[m[32m                    }[m
[32m+[m[32m                    catch (Exception E)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        notifyIcon.ShowBalloonTip(1000, "Writing StartWithWindows.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                //checkBox_TurboBoost[m
[32m+[m[32m                private void checkBox_TurboBoost_CheckedChanged(object sender, EventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\TurboBoost.txt";[m
[32m+[m[32m                    try[m
[32m+[m[32m                    {[m
[32m+[m[32m                        File.WriteAllText(path, checkBox_TurboBoost.Checked.ToString());[m
[32m+[m[32m                    }[m
[32m+[m[32m                    catch (Exception E)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        notifyIcon.ShowBalloonTip(1000, "Writing TurboBoost.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                //ShowTime[m
[32m+[m[32m                private void checkBoxShowTime_CheckedChanged(object sender, EventArgs e)[m
[32m+[m[32m                {[m
