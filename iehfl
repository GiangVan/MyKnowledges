[32m+[m[32m                //n·∫°p d·ªØ li·ªáu[m
[32m+[m[32m                ReadFiles();[m
[32m+[m[32m                //ƒë√≥ng Setting[m
[32m+[m[32m                panelSetting.Left = -210;[m
[32m+[m[32m                //n·∫°p d·ªØ li·ªáu cho treeView[m
[32m+[m[32m                FormLoad formLoad = new FormLoad(passwordBytes, treeView, !checkBox_TurboBoost.Checked, FormLogin);[m
[32m+[m[32m                if (!checkBox_TurboBoost.Checked)[m
[32m+[m[32m                    info = formLoad.info;[m
