[32m+[m[32m            //Change fonts[m
[32m+[m[32m            #region[m
[32m+[m[32m            //change font of textBoxContent[m
[32m+[m[32m            private void buttonChangeFont_textbox_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        FontDialog fontDialog = new FontDialog();[m
[32m+[m[32m                        fontDialog.Font = textBoxContent.Font;[m
[32m+[m[32m                        fontDialog.ShowDialog();[m
[32m+[m[32m                        if (fontDialog.Font != textBoxContent.Font)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            textBoxContent.Font = fontDialog.Font;[m
[32m+[m[32m                            try[m
[32m+[m[32m                            {[m
[32m+[m[32m                                trackBarSizeFont.Value = (int)textBoxContent.Font.Size;[m
[32m+[m[32m                            }[m
[32m+[m[32m                            catch[m
[32m+[m[32m                            {[m
[32m+[m[32m                                trackBarSizeFont.Value = 30;[m
[32m+[m[32m                            }[m
[32m+[m[32m                            string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\Font";[m
[32m+[m[32m                            if (!Directory.Exists(path)) Directory.CreateDirectory(path);[m
[32m+[m[32m                            try[m
[32m+[m[32m                            {[m
[32m+[m[32m                                FontConverter fontConverter = new FontConverter();[m
[32m+[m[32m                                File.WriteAllText(path + "\\textBoxContent.txt", fontConverter.ConvertToString(textBoxContent.Font));[m
[32m+[m[32m                            }[m
[32m+[m[32m                            catch (Exception E)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                notifyIcon.ShowBalloonTip(1000, "Writing textBoxContent.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //change font of treeView[m
[32m+[m[32m                    private void buttonChangeFont_listitems_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        FontDialog fontDialog = new FontDialog();[m
[32m+[m[32m                        fontDialog.Font = treeView.Font;[m
[32m+[m[32m                        fontDialog.ShowDialog();[m
[32m+[m[32m                        if (fontDialog.Font != treeView.Font)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            treeView.Font = fontDialog.Font;[m
[32m+[m[32m                            string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\Font";[m
[32m+[m[32m                            if (!Directory.Exists(path)) Directory.CreateDirectory(path);[m
[32m+[m[32m                            try[m
[32m+[m[32m                            {[m
[32m+[m[32m                                FontConverter fontConverter = new FontConverter();[m
[32m+[m[32m                                File.WriteAllText(path + "\\treeView.txt", fontConverter.ConvertToString(treeView.Font));[m
[32m+[m[32m                            }[m
[32m+[m[32m                            catch (Exception E)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                notifyIcon.ShowBalloonTip(1000, "Writing treeView.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
