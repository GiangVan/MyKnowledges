[32m+[m[32mÔªøusing System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.ComponentModel;[m
[32m+[m[32musing System.Data;[m
[32m+[m[32musing System.Drawing;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Text.RegularExpressions;[m
[32m+[m[32musing System.Windows.Forms;[m
[32m+[m
[32m+[m[32mnamespace YourExperience[m
[32m+[m[32m{[m
[32m+[m[32m    public partial class FormPassword : Form[m
[32m+[m[32m    {[m
[32m+[m[32m        private byte[] passwordBytes;[m
[32m+[m
[32m+[m[32m        public FormPassword(Point location, string text)[m
[32m+[m[32m        {[m
[32m+[m[32m            InitializeComponent();[m
[32m+[m[32m            Location = location;[m
[32m+[m[32m            Text = text;[m
[32m+[m[32m            ShowDialog();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public byte[] GetPassword()[m
[32m+[m[32m        {[m
[32m+[m[32m            return passwordBytes;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m        private void textBox1_TextChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            textBox1.Text = CheckChars(textBox1.Text);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void textBox2_TextChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            textBox2.Text = CheckChars(textBox2.Text);[m
[32m+[m[32m        }[m
[32m+[m[32m        private string CheckChars(string value)[m
[32m+[m[32m        {[m
[32m+[m[32m            Regex regex = new Regex("[^!-~]");[m
[32m+[m[32m            if (regex.Matches(value).Count != 0)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormNotification("Special character detection: " + value.Substring(value.Length - 1, 1), false);[m
[32m+[m[32m                value = "";[m
[32m+[m[32m            }[m
[32m+[m[32m            return value;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void button1_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (textBox2.Text != textBox1.Text)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormNotification("Those passwords didn't match!\nTry again?", false);[m
[32m+[m[32m                textBox2.Select();[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (textBox2.Text.Length < 8)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormNotification("Number of chars must be more than EIGHT chars", false);[m
[32m+[m[32m                textBox1.Select();[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            passwordBytes = Encoding.ASCII.GetBytes(textBox2.Text);[m
[32m+[m[32m            Close();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void textBox1_KeyDown(object sender, KeyEventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (e.Control && e.KeyCode == Keys.C)[m
[32m+[m[32m                if (textBox1.TextLength > 0) Clipboard.SetText(textBox1.Text);[m
[32m+[m[32m            if (e.Control && e.KeyCode == Keys.A)[m
[32m+[m[32m                textBox1.SelectAll();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void textBox2_KeyDown(object sender, KeyEventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (e.Control && e.KeyCode == Keys.C)[m
[32m+[m[32m                if (textBox2.TextLength > 0) Clipboard.SetText(textBox2.Text);[m
[32m+[m[32m            if (e.Control && e.KeyCode == Keys.A)[m
[32m+[m[32m                textBox2.SelectAll();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void pictureBox1_MouseEnter(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            pictureBox1.BackgroundImage = Properties.Resources.eye;[m
[32m+[m[32m            textBox1.UseSystemPasswordChar = false;[m
[32m+[m[32m            textBox2.UseSystemPasswordChar = false;[m
[32m+[m[32m            textBox1.SelectionStart = textBox1.TextLength;[m
[32m+[m[32m            textBox2.SelectionStart = textBox2.TextLength;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void pictureBox1_MouseLeave(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            pictureBox1.BackgroundImage = Properties.Resources.blind_eye_sign;[m
[32m+[m[32m            textBox1.UseSystemPasswordChar = true;[m
[32m+[m[32m            textBox2.UseSystemPasswordChar = true;[m
[32m+[m[32m            textBox1.SelectionStart = textBox1.TextLength;[m
[32m+[m[32m            textBox2.SelectionStart = textBox2.TextLength;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
