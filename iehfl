[32m+[m[32m                //di chuy·ªÉn m·ªôt TreeNode[m
[32m+[m[32m                #region move a TreeNode[m
[32m+[m[32m                    //di chuy·ªÉn TreeNode l√™n tr√™n c√πng[m
[32m+[m[32m                    #region onTopToolStripMenuItem_Click[m
[32m+[m[32m                        private void topToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (treeView.SelectedNode.Index == 0) return;[m
[32m+[m[32m                            TreeNode a = treeView.SelectedNode;[m
[32m+[m[32m                            TreeNode b = treeView.SelectedNode.Parent;[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            b.Nodes.Insert(0, a);[m
[32m+[m[32m                            treeView.SelectedNode = a;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    #endregion[m
[32m+[m[32m                    //di chuy·ªÉn TreeNode xu·ªëng cu·ªëi c√πng[m
[32m+[m[32m                    #region botToolStripMenuItem_Click[m
[32m+[m[32m                        private void botToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (treeView.SelectedNode.Index == treeView.SelectedNode.Parent.Nodes.Count - 1) return;[m
[32m+[m[32m                            TreeNode a = treeView.SelectedNode;[m
[32m+[m[32m                            int index = treeView.SelectedNode.Parent.Nodes.Count;[m
[32m+[m[32m                            TreeNode b = treeView.SelectedNode.Parent;[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            b.Nodes.Insert(index, a);[m
[32m+[m[32m                            treeView.SelectedNode = a;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    #endregion[m
[32m+[m[32m                    //di chuy·ªÉn TreeNode l√™n 1 ƒë∆°n v·ªã[m
[32m+[m[32m                    #region upToolStripMenuItem_Click[m
[32m+[m[32m                        private void upToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (treeView.SelectedNode.Index == 0) return;[m
[32m+[m[32m                            TreeNode a = treeView.SelectedNode;[m
[32m+[m[32m                            int index = treeView.SelectedNode.Index - 1;[m
[32m+[m[32m                            TreeNode b = treeView.SelectedNode.Parent;[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            b.Nodes.Insert(index, a);[m
[32m+[m[32m                            treeView.SelectedNode = a;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    #endregion[m
[32m+[m[32m                    //di chuy·ªÉn TreeNode xu·ªëng 1 ƒë∆°n v·ªã[m
[32m+[m[32m                    #region downToolStripMenuItem_Click[m
[32m+[m[32m                        private void downToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (treeView.SelectedNode.Index == treeView.SelectedNode.Parent.Nodes.Count - 1) return;[m
[32m+[m[32m                            TreeNode a = treeView.SelectedNode;[m
[32m+[m[32m                            int index = treeView.SelectedNode.Index + 1;[m
[32m+[m[32m                            TreeNode b = treeView.SelectedNode.Parent;[m
[32m+[m[32m                            treeView.SelectedNode.Remove();[m
[32m+[m[32m                            b.Nodes.Insert(index, a);[m
[32m+[m[32m                            treeView.SelectedNode = a;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    #endregion[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //c·∫Øt m·ªôt TreeNode[m
[32m+[m[32m                #region moveToolStripMenuItem_Click[m
[32m+[m[32m                    //ƒë·ªëi v·ªõi tree Node[m
[32m+[m[32m                    private void moveToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        //d√°n[m
[32m+[m[32m                        if (moveToolStripMenuItem.Text == "Paste  (of cutting)")[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if(cutedNode != treeView.SelectedNode)//ki·ªÉm tra xem c√≥ d√°n v√†o ch√≠nh n√≥ hay kh√¥ng[m
[32m+[m[32m                            {[m
[32m+[m[32m                                cutedNode.Remove();[m
[32m+[m[32m                                cutedNode.Text = PreventDuplication(cutedNode.Text, treeView.SelectedNode);[m
[32m+[m[32m                                treeView.SelectedNode.Nodes.Add(cutedNode);[m
[32m+[m[32m                                treeView.SelectedNode.Expand();[m
[32m+[m[32m                                treeView.SelectedNode.BackColor = Color.Gainsboro;[m
[32m+[m[32m                                treeView.SelectedNode.ForeColor = Color.DimGray;[m
[32m+[m[32m                                treeView.SelectedNode = cutedNode;[m
[32m+[m[32m                                treeView.SelectedNode.BackColor = SystemColors.Highlight;[m
[32m+[m[32m                                treeView.SelectedNode.ForeColor = SystemColors.Window;[m
[32m+[m[32m                                ShowTreeNodeContent(treeView.SelectedNode);[m
[32m+[m[32m                            }[m
[32m+[m[32m                            cutedNode.ForeColor = Color.DimGray;[m
[32m+[m[32m                            cutedNode = null;[m
[32m+[m[32m                            moveToolStripMenuItem.Text = "Cut";[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //c·∫Øt[m
[32m+[m[32m                        else[m
[32m+[m[32m                        {[m
[32m+[m[32m                            treeView.SelectedNode.ForeColor = Color.LightSeaGreen;[m
[32m+[m[32m                            moveToolStripMenuItem.Text = "Paste  (of cutting)";[m
[32m+[m[32m                            cutedNode = treeView.SelectedNode;[m
[32m+[m[32m                            treeView.SelectedNode = null;[m
[32m+[m[32m                            cutedNode.BackColor = Color.Gainsboro;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //ƒë·ªëi v·ªõi root Node[m
[32m+[m[32m                    private void pasteToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        cutedNode.Remove();[m
[32m+[m[32m                        cutedNode.Text = PreventDuplication(cutedNode.Text, treeView.SelectedNode);[m
[32m+[m[32m                        treeView.SelectedNode.Nodes.Add(cutedNode);[m
[32m+[m[32m                        treeView.SelectedNode.Expand();[m
[32m+[m[32m                        treeView.SelectedNode.BackColor = Color.Gainsboro;[m
[32m+[m[32m                        treeView.SelectedNode.ForeColor = Color.DimGray;[m
[32m+[m[32m                        treeView.SelectedNode = cutedNode;[m
[32m+[m[32m                        treeView.SelectedNode.BackColor = SystemColors.Highlight;[m
[32m+[m[32m                        treeView.SelectedNode.ForeColor = SystemColors.Window;[m
[32m+[m[32m                        ShowTreeNodeContent(treeView.SelectedNode);[m
[32m+[m[32m                        cutedNode.ForeColor = Color.DimGray;[m
[32m+[m[32m                        cutedNode = null;[m
[32m+[m[32m                        moveToolStripMenuItem.Text = "Cut";[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //[m
[32m+[m[32m                    private string PreventDuplication(string text, TreeNode ParentNode)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        while (true)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            again:[m
[32m+[m[32m                            foreach (TreeNode a_node in ParentNode.Nodes)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                if (a_node.Text == text)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    text += " - Copy";[m
[32m+[m[32m                                    goto again;[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                            return text;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //sao ch√©p m·ªôt TreeNode[m
[32m+[m[32m                #region[m
[32m+[m[32m                    //ƒë·ªëi v·ªõi tree node[m
[32m+[m[32m                    private void copyToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        Copy_a_TreeNode();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //ƒë·ªëi v·ªõi tree root[m
[32m+[m[32m                    private void toolStripMenuItem10_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        Copy_a_TreeNode();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void Copy_a_TreeNode()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        //d√°n[m
[32m+[m[32m                        if (copyToolStripMenuItem.Text == "Paste  (of copying)")[m
[32m+[m[32m                        {[m
[32m+[m[32m                            copiedNode = GetCopy_a_TreeNode(copiedNode);[m
[32m+[m[32m                            copiedNode.Text = PreventDuplication(copiedNode.Text, treeView.SelectedNode);[m
[32m+[m[32m                            treeView.SelectedNode.Nodes.Add(copiedNode);[m
[32m+[m[32m                            treeView.SelectedNode.Expand();[m
[32m+[m[32m                            treeView.SelectedNode.BackColor = Color.Gainsboro;[m
[32m+[m[32m                            treeView.SelectedNode.ForeColor = Color.DimGray;[m
[32m+[m[32m                            treeView.SelectedNode = copiedNode;[m
[32m+[m[32m                            treeView.SelectedNode.BackColor = SystemColors.Highlight;[m
[32m+[m[32m                            treeView.SelectedNode.ForeColor = SystemColors.Window;[m
[32m+[m[32m                            ShowTreeNodeContent(treeView.SelectedNode);[m
[32m+[m[32m                            copiedNode = null;[m
[32m+[m[32m                            copyToolStripMenuItem.Text = "Copy";[m
[32m+[m[32m                            toolStripMenuItem10.Text = "Copy";[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //sao ch√©p[m
[32m+[m[32m                        else[m
[32m+[m[32m                        {[m
[32m+[m[32m                            copyToolStripMenuItem.Text = "Paste  (of copying)";[m
[32m+[m[32m                            toolStripMenuItem10.Text = "Paste  (of copying)";[m
[32m+[m[32m                            copiedNode = treeView.SelectedNode;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //ƒë·ªá quy gi√∫p sao ch√©p m·ªôt treenode[m
[32m+[m[32m                    private TreeNode GetCopy_a_TreeNode(TreeNode root)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        TreeNode new_root = new TreeNode(root.Text);[m
[32m+[m[32m                        if (root.Name != "!2||-@#x")[m
[32m+[m[32m                        {[m
[32m+[m[32m                            //n·∫øu l√† UnlockedNode[m
[32m+[m[32m                            if (root.Name.Contains(nodesCode))[m
[32m+[m[32m                            {[m
[32m+[m[32m                                SaveAndLockTheUnlockedTreeNode(root);[m
[32m+[m[32m                            }[m
[32m+[m[32m                            //n·∫øu l√† LockedNode[m
[32m+[m[32m                            if (root.Name.Contains(lockCode))[m
[32m+[m[32m                            {[m
[32m+[m[32m                                new_root.Tag = root.Tag;[m
[32m+[m[32m                                //n·∫øu Node b·ªã copy kh√¥ng c√≥ d·ªØ li·ªáu (c√≥ th·ªÉ do ƒë·ªçc file trong qu√° tr√¨nh kh·ªüi ƒë·ªông ch∆∞∆°ng tr√¨nh th·∫•t b·∫°i)[m
[32m+[m[32m                                if (new_root.Tag == null)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    new_root.Name = Application.StartupPath + "\\Data\\" + root.Name.Substring(0, root.Name.LastIndexOf(lockCode));[m
[32m+[m[32m                                    return new_root;[m
[32m+[m[32m                                }[m
[32m+[m[32m                                //t·∫°o th∆∞ m·ª•c ƒë·ªÉ l∆∞u file (n·∫øu ch∆∞a c√≥)[m
[32m+[m[32m                                string path = Application.StartupPath + "\\Data\\";[m
[32m+[m[32m                                if (!Directory.Exists(path))[m
[32m+[m[32m                                    Directory.CreateDirectory(path);[m
[32m+[m[32m                                //t·∫°o t√™n file[m
[32m+[m[32m                                string fileName = DateTime.Now.Hour + "-" + DateTime.Now.Minute + "-" + DateTime.Now.Second + "-" + MousePosition.X.ToString() + "-" + MousePosition.Y.ToString() + "-" + root.GetHashCode().ToString() + GetHashCode().ToString() + random.Next(-2147483648, 2147483647).ToString() + DateTime.Now.Ticks.ToString().Substring(12) + "-" + DateTime.Now.Day + "-" + DateTime.Now.Month + "-" + DateTime.Now.Year + ".g";[m
[32m+[m[32m                                //ti·∫øn h√†nh l∆∞u[m
[32m+[m[32m                                WriteTheFile:[m
[32m+[m[32m                                try[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    File.WriteAllBytes(path + fileName, ((DataBytes)new_root.Tag).bytes);[m
[32m+[m[32m                                }[m
[32m+[m[32m                                catch (Exception E)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    textBoxContent.Text = "Can't write the file:\t" + path + fileName + "\r\n\r\n" + E.ToString();[m
[32m+[m[32m                                    if (MessageBox.Show("Can't write the file: " + fileName + "\n" + E.ToString(), "ERROR", MessageBoxButtons.RetryCancel, MessageBoxIcon.Error) == DialogResult.Retry)[m
[32m+[m[32m                                        goto WriteTheFile;[m
[32m+[m[32m                                }[m
[32m+[m[32m                                new_root.Name = fileName + lockCode;[m
[32m+[m[32m                            }[m
[32m+[m[32m                            //n·∫øu l√† Node th∆∞·ªùng[m
[32m+[m[32m                            else[m
[32m+[m[32m                                new_root.Name = root.Name;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        foreach (TreeNode a_node in root.Nodes)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            new_root.Nodes.Add(GetCopy_a_TreeNode(a_node));[m
[32m+[m[32m                        }[m
[32m+[m[32m                        return new_root;[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //x·ªï ra t·∫•t c·∫£ c√°c TreeNode con c·ªßa c√°c TreeNode cha[m
[32m+[m[32m                #region expandAllItemsToolStripMenuItem_Click[m
[32m+[m[32m                    private void expandAllItemsToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        treeView.SelectedNode.ExpandAll();[m
[32m+[m[41m            [m
[32m+[m[32m                }[m
