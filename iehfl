[32m+[m[32m            private void ReadFiles()[m
[32m+[m[32m        {[m
[32m+[m[32m            string fileName;[m
[32m+[m[32m            string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\";[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                //l∆∞u k√≠ch th∆∞·ªõc form[m[41m [m
[32m+[m[32m                #region[m
[32m+[m[32m                    fileName = "WindowState.txt";[m
[32m+[m[32m                    if (File.Exists(path + fileName))[m
[32m+[m[32m                {[m
[32m+[m[32m                    try[m
[32m+[m[32m                    {[m
[32m+[m[32m                        string str = File.ReadAllText(path + fileName);[m
[32m+[m[32m                        int width, height;[m
[32m+[m[32m                        string[] str2 = new string[2];[m
[32m+[m[32m                        if (str.Length > 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (str == "Maximized") WindowState = FormWindowState.Maximized;[m
[32m+[m[32m                            else[m
[32m+[m[32m                            {[m
[32m+[m[32m                                str = "Size.txt";[m
[32m+[m[32m                                if (File.Exists(path + str) && File.ReadAllText(path + str).Length > 0)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    str2 = File.ReadAllLines(path + str);[m
[32m+[m[32m                                    width = int.Parse(str2[0]);[m
[32m+[m[32m                                    height = int.Parse(str2[1]);[m
[32m+[m[32m                                    if (width > 160 && width < Screen.PrimaryScreen.Bounds.Width) Width = width;[m
[32m+[m[32m                                    if (height > 28 && height < Screen.PrimaryScreen.Bounds.Height) Height = height;[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    catch (Exception e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        notifyIcon.ShowBalloonTip(1000, "Reading " + fileName + " file failed!", e.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                #endregion[m
[32m+[m[32m                //ShowTime[m
[32m+[m[32m                #region[m
[32m+[m[32m                    fileName = "ShowTime.txt";[m
[32m+[m[32m                    if (File.Exists(path + fileName))[m
[32m+[m[32m                {[m
[32m+[m[32m                    try[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (File.ReadAllText(path + fileName).Length > 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            checkBoxShowTime.Checked = bool.Parse(File.ReadAllText(path + fileName));[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    catch (Exception e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        notifyIcon.ShowBalloonTip(1000, "Reading " + fileName + " file failed!", e.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                #endregion[m
[32m+[m[32m                //checkBox_TurboBoost[m
[32m+[m[32m                #region[m
[32m+[m[32m                    fileName = "TurboBoost.txt";[m
[32m+[m[32m                    if (File.Exists(path + fileName))[m
[32m+[m[32m                    {[m
[32m+[m[32m                        try[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (File.ReadAllText(path + fileName).Length > 0)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                checkBox_TurboBoost.Checked = bool.Parse(File.ReadAllText(path + fileName));[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        catch (Exception e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            notifyIcon.ShowBalloonTip(1000, "Reading " + fileName + " file failed!", e.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m[32m                //AutoSave[m
[32m+[m[32m                #region[m
