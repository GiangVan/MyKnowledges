[32m+[m[32m                        OpenFileDialog openFileDialog = new OpenFileDialog();[m
[32m+[m[32m                        if (openFileDialog.ShowDialog() == DialogResult.OK && openFileDialog.FileName.Length > 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            SetClipboardAndPaste(openFileDialog.FileName);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //Path of a Folder[m
