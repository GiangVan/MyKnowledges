[32m+[m[32m                                        {[m
[32m+[m[32m                                            textBoxContent.TextAlign = HorizontalAlignment.Center;[m
[32m+[m[32m                                            rightToolStripMenuItem.Image = null;[m
[32m+[m[32m                                            leftToolStripMenuItem.Image = null;[m
[32m+[m[32m                                            centerToolStripMenuItem.Image = Properties.Resources._checked;[m
[32m+[m[32m                                        }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //Run the highlighting text[m
[32m+[m[32m                #region[m
[32m+[m[32m                    private void runTheTextToolStripMenuItem_Click_1(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (textBoxContent.SelectedText.Length > 0)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                try[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    Process.Start(textBoxContent.SelectedText);[m
[32m+[m[32m                                }[m
[32m+[m[32m                                catch[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    Process.Start("https://www.google.com/search?q=" + textBoxContent.SelectedText);[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //Find and Replace[m
[32m+[m[32m                #region[m
[32m+[m[32m                    private void textBoxContent_TextChanged(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
