[32m+[m[41m	 [m
[32m+[m	[32mclass AnimationMOVE[m
[32m+[m	[32m{[m
[32m+[m[32m        //khai b√°o[m
[32m+[m[32m        #region[m
[32m+[m[32m            private int index;[m
[32m+[m[32m            private bool is_increase;[m
[32m+[m[32m            private bool is_vertical = new bool();[m
[32m+[m[32m            private Control control;[m
[32m+[m[32m            private int speedTime = 10;[m
[32m+[m[32m            private string HashCode;//d√πng ƒë·ªÉ nh·∫≠n bi·∫øt xem control c√≥ ƒëung ƒë∆∞·ª£c AnimationMOVE hay kh√¥ng, n·∫øu control.Name == HashCode l√† c√≥ v√† ng∆∞·ª£c l·∫°i[m
[32m+[m[32m            private int delayTime = 0;[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        //constructors[m
[32m+[m[32m        #region[m
[32m+[m[32m            public AnimationMOVE(Control control, int index, bool is_increase)[m
[32m+[m		[32m    {[m
[32m+[m[32m                //control.Enabled = false;[m
[32m+[m[32m                this.is_increase = is_increase;[m
[32m+[m[32m                this.index = index;[m
[32m+[m			[32m    this.control = control;[m
[32m+[m[32m                HashCode = control.GetHashCode().ToString() + GetHashCode().ToString();[m
[32m+[m[32m                control.Name = HashCode;[m
[32m+[m[32m                Thread thread = new Thread(Run);[m
[32m+[m[32m                //thread.Name = "AnimationMOVE (" + GetHashCode() + ")";[m
[32m+[m[32m                thread.Start();[m
[32m+[m		[32m    }[m
[32m+[m[32m            public AnimationMOVE(Control control, int index, bool is_increase,bool is_vertical, int delayTime, int speedTime)[m
[32m+[m[32m            {[m
[32m+[m[32m            //control.Enabled = false;[m
[32m+[m[32m            this.speedTime = speedTime;[m
[32m+[m[32m            this.delayTime = delayTime;[m
[32m+[m[32m            this.is_vertical = is_vertical;[m
[32m+[m[32m            this.is_increase = is_increase;[m
[32m+[m[32m            this.index = index;[m
[32m+[m[32m            this.control = control;[m
[32m+[m[32m            HashCode = control.GetHashCode().ToString() + GetHashCode().ToString();[m
[32m+[m[32m            control.Name = HashCode;[m
[32m+[m[32m            Thread thread = new Thread(Run);[m
[32m+[m[32m            //thread.Name = "AnimationMOVE (" + GetHashCode() + ")";[m
[32m+[m[32m            thread.Start();[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        private void Run()// y = sin(((x-1)^4) * œÄ) l·∫•y trong kho·∫£ng t·ª´ 0 ƒë·∫øn 1.[m
[32m+[m[32m        {[m
[32m+[m[32m            if (delayTime > 0) Thread.Sleep(delayTime);[m
[32m+[m[32m            if (control.Name != HashCode) return; //n·∫øu control n√†y ƒë∆∞·ª£c AnimationMOVE ·ªü m·ªôt lu·ªìng kh√°c th√¨ out[m
[32m+[m[32m            try//khi ch∆∞∆°ng tr√¨nh ƒë√≥ng m√† form Info v·∫´n ch∆∞a b·∫≠t th√¨ s·∫Ω ƒë√≥ng form info nh∆∞ng form l·∫°i g·ªçi h√†m ƒë√≥ng SETTING n√™n s·∫Ω g√¢y ra l·ªói v√¨ formMain l√∫c ƒë√≥ ƒë√£ ƒë√≥ng r·ªìi.[m
[32m+[m			[32m{[m
[32m+[m[32m                double speed = new double();[m
[32m+[m				[32mdouble extraVAR;[m
[32m+[m				[32mint n = 20;//s√¥ l·∫ßn l·∫∑p.[m
[32m+[m
[32m+[m[32m                if(is_vertical)[m
[32m+[m[32m                    extraVAR = (index - control.Top) / 5.98;//bi·∫øn ph·ª•.[m
[32m+[m[32m                else[m
[32m+[m[32m                    extraVAR = (index - control.Left) / 6.3;//bi·∫øn ph·ª•.[m
[32m+[m
[32m+[m[32m                for (int i = 0; i < n; i++)[m
[32m+[m[32m                {[m
[32m+[m[41m                    [m
[32m+[m[32m                    if (control.Name != HashCode) return; //n·∫øu control n√†y ƒë∆∞·ª£c AnimationMOVE ·ªü m·ªôt lu·ªìng kh√°c th√¨ out[m
[32m+[m
[32m+[m[32m                    if(is_increase || (is_vertical && !is_increase))[m
[32m+[m[32m                        speed = extraVAR * (Math.Sin(Math.Pow((i / (double)n) - 1, 4) * Math.PI));//s√†i t·∫°m v·ªõi n=20.[m
[32m+[m[32m                    else[m
[32m+[m[32m                        speed = extraVAR * (Math.Sin(Math.Pow((i / (double)n) - 0, 4) * Math.PI));//s√†i t·∫°m v·ªõi n=20.[m
[32m+[m[32m                    control.Invoke(new MethodInvoker(delegate ()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (is_vertical)[m
[32m+[m[32m                            control.Top += (int)speed;[m
[32m+[m[32m                        else[m
[32m+[m[32m                            control.Left += (int)speed;[m
[32m+[m[32m                    }));[m
[32m+[m
[32m+[m[32m                    Thread.Sleep(speedTime);[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                control.Invoke(new MethodInvoker(delegate ()[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (is_vertical)[m
[32m+[m[32m                        control.Top = index;[m
[32m+[m[32m                    else[m
[32m+[m[32m                        control.Left = index;[m
[32m+[m[32m                    //control.Enabled = true;[m
[32m+[m[32m                    //if (is_increase && is_vertical) MessageBox.Show(control.Top.ToString());[m
[32m+[m[32m                }));[m
[32m+[m[32m            }[m
[32m+[m			[32mcatch { }[m[41m				[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
