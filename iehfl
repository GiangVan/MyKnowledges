[32m+[m
[32m+[m[32m        //chung[m
[32m+[m[32m        #region[m
[32m+[m[32m            private void SetClipboardAndPaste(string text)[m
[32m+[m[32m            {[m
[32m+[m[32m                string old_str = Clipboard.GetText();[m
[32m+[m[32m                if(text.Length > 0)[m
[32m+[m[32m                {[m
[32m+[m[32m                    Clipboard.SetText(text);[m
[32m+[m[32m                    SendKeys.SendWait("^V");[m
[32m+[m[32m                    if (old_str.Length > 0) Clipboard.SetText(old_str);[m
[32m+[m[32m                    else Clipboard.Clear();[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            private void UnSelectedNode()[m
[32m+[m[32m        {[m
[32m+[m[32m            if(treeView.SelectedNode != null)[m
[32m+[m[32m            {[m
[32m+[m[32m                treeView.SelectedNode.BackColor = Color.Gainsboro;[m
[32m+[m[32m                if (treeView.SelectedNode != cutedNode) treeView.SelectedNode.ForeColor = Color.DimGray;[m
[32m+[m[32m                treeView.SelectedNode = null;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m            private void SaveAndLockTheUnlockedTreeNode(TreeNode node)[m
[32m+[m[32m            {[m
[32m+[m[32m                UseWaitCursor = true;[m
[32m+[m[32m                byte[] password = Encoding.Default.GetBytes(node.Name.Substring(node.Name.LastIndexOf(lockCode) + lockCode.Length).ToCharArray());[m
[32m+[m[32m                string text_content = node.Name.Substring(0, node.Name.IndexOf(nodesCode));[m
[32m+[m[32m                //ƒë∆∞a TreeNode v·ªÅ tr·∫°ng th√°i ti·ªÅn Locked (TreeNode.Name = path + lockCode)[m
[32m+[m[32m                node.Name = node.Name.Substring(node.Name.LastIndexOf(pathCode) + pathCode.Length, node.Name.LastIndexOf(lockCode) + lockCode.Length - node.Name.LastIndexOf(pathCode) - pathCode.Length);[m
[32m+[m[32m                //b·∫Øt ƒë·∫ßu ti·ªÅn h√†nh c√°c b∆∞·ªõc ƒë·ªÉ l∆∞u d·ªØ li·ªáu v√† lock TreeNode l·∫°i[m
[32m+[m[32m                //t·∫°o th∆∞ m·ª•c ƒë·ªÉ l∆∞u file (n·∫øu ch∆∞a c√≥)[m
[32m+[m[32m                string path = Application.StartupPath + "\\Data\\";[m
[32m+[m[32m                if (!Directory.Exists(path))[m
[32m+[m[32m                    Directory.CreateDirectory(path);[m
[32m+[m[32m                //l·∫•y ƒë∆∞·ªùng d·∫´n ƒë·ªÉ l∆∞u TreeNode[m
[32m+[m[32m                string fileName = node.Name.Substring(0, node.Name.LastIndexOf(lockCode));[m
[32m+[m[32m                //chuy·ªÉn c√°c TreeNodes con c·ªßa TreeNode ƒë√≥ th√†nh chu·ªói[m[41m		[m
[32m+[m[32m                text_nodes = "";[m
[32m+[m[32m                TreeNodeToString(node);[m
[32m+[m[32m                text_nodes += rightCode;[m
[32m+[m[32m                //t·∫°o bytes d·ªØ li·ªáu coded_text[m
[32m+[m[32m                byte[] bytes = GCoding.Encoding(Encoding.Unicode.GetBytes((passCode + text_content + nodesCode + text_nodes).ToCharArray()), password);[m
[32m+[m[32m                //ti·∫øn h√†nh l∆∞u[m
[32m+[m[32m                WriteTheFile:[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    File.WriteAllBytes(path + fileName, bytes);[m
[32m+[m[32m                }[m
[32m+[m[32m                catch (Exception E)[m
[32m+[m[32m                {[m
[32m+[m[32m                    textBoxContent.Text = "Can't write the file:\t" + path + fileName + "\r\n\r\n" + E.ToString();[m
[32m+[m[32m                    if (MessageBox.Show("Can't write the file: " + fileName + "\n" + E.ToString(), "ERROR", MessageBoxButtons.RetryCancel, MessageBoxIcon.Error) == DialogResult.Retry)[m
[32m+[m[32m                        goto WriteTheFile;[m
[32m+[m[32m                }[m
[32m+[m[32m                //ƒë∆∞a node v·ªÅ tr·∫°ng th√°i locked[m
[32m+[m[32m                textBoxContent.Text = "";[m
[32m+[m[32m                node.Tag = new DataBytes(bytes);[m
[32m+[m[32m                node.Nodes.Clear();[m
[32m+[m[32m                UseWaitCursor = false;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        private void textBoxContent_Leave(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if(textBoxContent.ReadOnly) return;[m
[32m+[m[32m            Text ="out";[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
