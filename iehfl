[32m+[m[32m        //m·ªçi th·ª© v·ªÅ textBoxContent[m
[32m+[m[32m        #region textBoxContent[m
[32m+[m
[32m+[m[32m            //thanh ch·ªânh size font nhanh[m
[32m+[m[32m            #region trackBarSizeFont[m
[32m+[m[32m                private void trackBarSizeFont_Scroll(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        textBoxContent.Font = new Font(textBoxContent.Font.FontFamily, trackBarSizeFont.Value, textBoxContent.Font.Style);[m
[32m+[m[32m                        string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\Font";[m
[32m+[m[32m                        if (!Directory.Exists(path)) Directory.CreateDirectory(path);[m
[32m+[m[32m                        try[m
[32m+[m[32m                        {[m
[32m+[m[32m                            FontConverter fontConverter = new FontConverter();[m
[32m+[m[32m                            File.WriteAllText(path + "\\textBoxContent.txt", fontConverter.ConvertToString(textBoxContent.Font));[m
[32m+[m[32m                        }[m
[32m+[m[32m                        catch (Exception E)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            notifyIcon.ShowBalloonTip(1000, "Writing textBoxContent.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m            #endregion[m
[32m+[m
[32m+[m[32m            //context menu strip[m
[32m+[m[32m            #region contextMenuStrip_textBoxContent[m
[32m+[m
[32m+[m[32m                //show context menu strip[m
[32m+[m[32m                #region show[m
[32m+[m[32m                    private void textBoxContent_MouseDown(object sender, MouseEventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (e.Button == MouseButtons.Left && textBoxContent.TextLength == 0) treeView.Select();[m
[32m+[m[32m                        if (e.Button == MouseButtons.Right)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            //undo[m
[32m+[m[32m                            if (textBoxContent.CanUndo) toolStripMenuItem_Undo.Enabled = true;[m
[32m+[m[32m                            else toolStripMenuItem_Undo.Enabled = false;[m
[32m+[m[32m                            //save[m
[32m+[m[32m                            if (!textBoxContent.ReadOnly) toolStripMenuItem_save.Enabled = true;[m
[32m+[m[32m                            else toolStripMenuItem_save.Enabled = false;[m
[32m+[m[32m                            //edit[m
[32m+[m[32m                            toolStripMenuItem_Edit.Enabled = false;[m
[32m+[m[32m                            if (treeView.SelectedNode != null && textBoxContent.ReadOnly)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                try[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    if (textBoxContent.Text == treeView.SelectedNode.Name.Substring(0, treeView.SelectedNode.Name.IndexOf(nodesCode))) toolStripMenuItem_Edit.Enabled = true;[m
[32m+[m[32m                                }[m
[32m+[m[32m                                catch { }[m
[32m+[m[32m                                if(textBoxContent.Text == treeView.SelectedNode.Name) toolStripMenuItem_Edit.Enabled = true;[m
[32m+[m[32m                            }[m
[32m+[m[32m                            //cut[m
[32m+[m[32m                            if(textBoxContent.SelectedText.Length == 0 || textBoxContent.ReadOnly) toolStripMenuItem_Cut.Enabled = false;[m
[32m+[m[32m                            else toolStripMenuItem_Cut.Enabled = true;[m
[32m+[m[32m                            //copy[m
[32m+[m[32m                            if (textBoxContent.SelectedText.Length == 0) toolStripMenuItem_Copy.Enabled = false;[m
[32m+[m[32m                            else toolStripMenuItem_Copy.Enabled = true;[m
[32m+[m[32m                            //paste[m
[32m+[m[32m                            if (textBoxContent.ReadOnly || Clipboard.GetText().Length == 0) toolStripMenuItem_Paste.Enabled = false;[m
[32m+[m[32m                            else toolStripMenuItem_Paste.Enabled = true;[m
[32m+[m[32m                            //cut rows[m
[32m+[m[32m                            if (textBoxContent.ReadOnly) cutRowsToolStripMenuItem.Enabled = false;[m
[32m+[m[32m                            else cutRowsToolStripMenuItem.Enabled = true;[m
[32m+[m[32m                            //select all[m
[32m+[m[32m                            if (textBoxContent.TextLength == 0) selectAllToolStripMenuItem.Enabled = false;[m
[32m+[m[32m                            else selectAllToolStripMenuItem.Enabled = true;[m
[32m+[m[32m                            //get[m
[32m+[m[32m                            if (textBoxContent.ReadOnly) getToolStripMenuItem.Enabled = false;[m
[32m+[m[32m                            else getToolStripMenuItem.Enabled = true;[m
[32m+[m[32m                            //find and replace[m
[32m+[m[32m                            if (FormFindAndReplace.Enabled) findCtrlFToolStripMenuItem.Enabled = false;[m
[32m+[m[32m                            else findCtrlFToolStripMenuItem.Enabled = true;[m
[32m+[m[32m                            //Run the highlighting text[m
[32m+[m[32m                            if (textBoxContent.SelectedText.Length == 0) runTheTextToolStripMenuItem.Enabled = false;[m
[32m+[m[32m                            else runTheTextToolStripMenuItem.Enabled = true;[m
[32m+[m[32m                            if (textBoxContent.ReadOnly) runTheTextToolStripMenuItem.Text = "Run the highlighting text   (R)";[m
[32m+[m[32m                            else runTheTextToolStripMenuItem.Text = "Run the highlighting text   (Ctrl + R)";[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //c√°c thao t√°c b√†n ph√≠m[m
[32m+[m[32m                #region textBoxContent_KeyDown[m
[32m+[m[32m                    private void textBoxContent_KeyDown(object sender, KeyEventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        e.Handled = true;[m
[32m+[m[32m                        //WordWrap[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.W)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (textBoxContent.WordWrap)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                wordWrapToolStripMenuItem.Image = Properties.Resources._checked;[m
[32m+[m[32m                                textBoxContent.WordWrap = false;[m
[32m+[m[32m                            }[m
[32m+[m[32m                            else[m
[32m+[m[32m                            {[m
[32m+[m[32m                                wordWrapToolStripMenuItem.Image = null;[m
[32m+[m[32m                                textBoxContent.WordWrap = true;[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //Cancel[m
[32m+[m[32m                        if(!textBoxContent.ReadOnly && e.KeyCode == Keys.Escape)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            CloseEditting();[m
[32m+[m[32m                            textBoxContent.Text = old_text;[m
[32m+[m[32m                            old_text = "";[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //Run the highlighting text                                         - Ctrl + R or R[m
[32m+[m[32m                        if (((e.Control && e.KeyCode == Keys.R && !textBoxContent.ReadOnly) || (e.KeyCode == Keys.R && textBoxContent.ReadOnly)) && textBoxContent.SelectedText.Length != 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            try[m
[32m+[m[32m                            {[m
[32m+[m[32m                                Process.Start(textBoxContent.SelectedText);[m
[32m+[m[32m                            }[m
[32m+[m[32m                            catch[m
[32m+[m[32m                            {[m
[32m+[m[32m                                Process.Start("https://www.google.com/search?q=" + textBoxContent.SelectedText);[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //Find and Replace                                                  - Ctrl + F[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.F && !FormFindAndReplace.Enabled)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            Show_FormFindAndReplace();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //xo√° c√°c d√≤ng ƒë√£ ch·ªçn                                              - Ctrl + L[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.L && !textBoxContent.ReadOnly)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            CutRows();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //l∆∞u                                                               - Ctrl + S[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.S && !textBoxContent.ReadOnly)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            SaveContent();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //b√¥i ƒëen to√†n b·ªô vƒÉn b·∫£n                                           - Ctrl + A[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.A)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            textBoxContent.SelectAll();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //ch·ªânh s·ª≠a m·ªôt Tree node                                           - Ctrl + D[m
[32m+[m[32m                        if (e.Control && e.KeyCode == Keys.D && treeView.SelectedNode != null && textBoxContent.ReadOnly)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            OpenEditContent_of_SelectedNode();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        //cƒÉn l·ªÅ b·∫±ng[m
[32m+[m[32m                        if (e.KeyCode == Keys.Enter && !textBoxContent.ReadOnly && textBoxContent.SelectionStart > 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                        string str = textBoxContent.Text.Substring(0, textBoxContent.SelectionStart);[m
[32m+[m[32m                        int index = str.LastIndexOf("\r\n");[m
[32m+[m[32m                        if (index == -1) index = 0;[m
[32m+[m[32m                        else index += 2;[m
[32m+[m[32m                        StringBuilder stringBuilder = new StringBuilder(str.Substring(index));[m
[32m+[m[32m                        string str2 = "";[m
[32m+[m[32m                        for (int i = 0; i < stringBuilder.Length; i++)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (stringBuilder.ToString(i, 1).Contains("\t")) str2 += "\t";[m
[32m+[m[32m                            else break;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        SetClipboardAndPaste(str2);[m
[32m+[m[32m                    }[m
[32m+[m[32m                        //tab 2 d√≤ng tr·ªü l√™n[m
[32m+[m[32m                        if (e.KeyCode == Keys.Tab && textBoxContent.SelectedText.Contains("\r\n") && !textBoxContent.ReadOnly)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                SeleteRows();[m
[32m+[m[32m                                string str = textBoxContent.SelectedText.Replace("\r\n", "\r\n\t");[m
[32m+[m[32m                                int index = textBoxContent.SelectionStart;[m
[32m+[m[32m                                SetClipboardAndPaste(str);[m
[32m+[m[32m                                textBoxContent.SelectionStart = index + 1;[m
[32m+[m[32m                                textBoxContent.SelectionLength = str.Length;[m
[32m+[m[32m                            }[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[41m                        [m
[32m+[m[41m                        [m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //undo[m
[32m+[m[32m                #region toolStripMenuItem_Undo[m
[32m+[m[32m                    private void toolStripMenuItem_Undo_Click(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            textBoxContent.Undo();[m
[32m+[m[32m                        }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //save[m
[32m+[m[32m                #region[m[41m [m
[32m+[m[32m                    private void toolStripMenuItem_save_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        SaveContent();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void SaveContent()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (CheckSpecialCharacter(nodesCode) || CheckSpecialCharacter(pathCode) || CheckSpecialCharacter(leftCode) || CheckSpecialCharacter(lockCode) || CheckSpecialCharacter(nameCode) || CheckSpecialCharacter(rightCode)) return;[m
[32m+[m[32m                        CloseEditting();[m
[32m+[m[32m                        //n·∫øu TreeNode ƒë√≥ ƒëang ·ªü tr·∫°ng th√°i unlocked th√¨ l∆∞u theo ki·ªÉu "locked"[m
[32m+[m[32m                        if (treeView.SelectedNode.Name.Contains(nodesCode))[m
[32m+[m[32m                            treeView.SelectedNode.Name = textBoxContent.Text + treeView.SelectedNode.Name.Substring(treeView.SelectedNode.Name.IndexOf(nodesCode));[m
[32m+[m[32m                        //n·∫øu kh√¥ng th√¨ l∆∞u theo ki·ªÉu b√¨nh th∆∞·ªùng[m
[32m+[m[32m                        else[m
[32m+[m[32m                            treeView.SelectedNode.Name = textBoxContent.Text;[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //edit[m
[32m+[m[32m                #region toolStripMenuItem_Edit[m
[32m+[m[32m                    private void toolStripMenuItem_Edit_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        OpenEditContent_of_SelectedNode();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void OpenEditContent_of_SelectedNode()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (treeView.SelectedNode == null) return;[m
[32m+[m[32m                        try[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (textBoxContent.Text == treeView.SelectedNode.Name || textBoxContent.Text == treeView.SelectedNode.Name.Substring(0, treeView.SelectedNode.Name.IndexOf(nodesCode)))[m
[32m+[m[32m                            {[m
[32m+[m[32m                                comboBoxSearch.Visible = false;[m
[32m+[m[32m                                panel_buttonSave.Visible = true;[m
[32m+[m[32m                                buttonCancel.Visible = true;[m
[32m+[m[32m                                treeView.Enabled = false;[m
[32m+[m[32m                                saveToolStripMenuItem1.Enabled = false;[m
[32m+[m[32m                                textBoxContent.ReadOnly = false;[m
[32m+[m[32m                                textBoxContent.Text = treeView.SelectedNode.Name;[m
[32m+[m[32m                                old_text = textBoxContent.Text;[m
[32m+[m[32m                                if(treeView.SelectedNode.Name.Contains(nodesCode)) textBoxContent.Text = treeView.SelectedNode.Name.Substring(0, treeView.SelectedNode.Name.IndexOf(nodesCode));[m
[32m+[m[32m                                old_text = textBoxContent.Text;[m
[32m+[m[32m                                textBoxContent.Select();[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        catch { }[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //select all[m
[32m+[m[32m                #region[m
[32m+[m[32m                    private void selectAllToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    textBoxContent.Select();[m
[32m+[m[32m                    textBoxContent.SelectAll();[m
[32m+[m[32m                }[m
[32m+[m[32m                #endregion[m
[32m+[m
