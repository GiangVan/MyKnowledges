[32m+[m[32m        //constructor ƒë·ªçc d·ªØ li·ªáu[m
[32m+[m[32m        public FormLoad(byte[] password, TreeView treeView, bool run_ShowComputer, Form FormLogin)[m
[32m+[m		[32m{[m
[32m+[m[32m            InitializeComponent();[m
[32m+[m[32m            Left = FormLogin.Left + FormLogin.Width / 2 - Width / 2;[m
[32m+[m[32m            Top = FormLogin.Top + FormLogin.Height / 2 - Height / 2;[m
[32m+[m[32m            this.run_ShowComputer = run_ShowComputer;[m
[32m+[m			[32mthis.password = password;[m
[32m+[m			[32mthis.treeView = treeView;[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                System.Threading.Thread thread = new System.Threading.Thread(ReadFile);[m
[32m+[m[32m                thread.Start();[m
[32m+[m[32m                ShowDialog();[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception e)[m
[32m+[m[32m            {[m
[32m+[m[32m                MessageBox.Show(e.ToString(), "ERROR", MessageBoxButtons.OK, MessageBoxIcon.Error);[m
[32m+[m[32m                Application.Exit();[m
[32m+[m[32m            }[m
[32m+[m[41m			[m
[32m+[m		[32m}[m
[32m+[m[32m        //constructor l∆∞u d·ªØ li·ªáu[m
[32m+[m[32m        public FormLoad(byte[] password, TreeView treeView, Form FormMain, NotifyIcon notifyIcon)[m
[32m+[m		[32m{[m
[32m+[m			[32mInitializeComponent();[m
[32m+[m[32m            Left = FormMain.Left + FormMain.Width / 2 - Width / 2;[m
[32m+[m[32m            Top = FormMain.Top + FormMain.Height / 2 - Height / 2;[m
[32m+[m[32m            this.password = password;[m
[32m+[m			[32mthis.treeView = treeView;[m
[32m+[m[32m            this.notifyIcon = notifyIcon;[m
[32m+[m
[32m+[m[32m            FormMain.Cursor = Cursors.AppStarting;[m
[32m+[m[32m            //chuy·ªÉn treeView th√†nh chu·ªói[m[41m			[m
[32m+[m[32m            foreach (TreeNode node in treeView.Nodes)[m
[32m+[m[32m            {[m
[32m+[m[32m                if (node.Nodes == null)[m
[32m+[m[32m                    break;[m
[32m+[m[32m                TreeNodeToString(node, false);[m
[32m+[m[32m                savingString += rightCode;[m
[32m+[m[32m                break;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (savingString.Length == 0 || (dataString == savingString && password == passwordBytes))[m
[32m+[m[32m            {[m
[32m+[m[32m                FormMain.Cursor = Cursors.Default;[m
[32m+[m[32m                return;//tho√°t ngay n·∫øu d·ªØ li·ªáu kh√¥ng c√≥ g√¨[m
[32m+[m[32m            }[m
[32m+[m[32m            //[m
[32m+[m[32m            FormMain.Cursor = Cursors.Default;[m
[32m+[m
[32m+[m[32m            System.Threading.Thread thread = new System.Threading.Thread(SaveFile);[m
[32m+[m			[32mthread.Start();[m
[32m+[m			[32mShowDialog();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m        //ƒë·ªçc file v√† n·∫°p treeView[m[41m	[m
[32m+[m[32m        #region ReadFile[m[41m																																																																		[m
[32m+[m[32m        private void ReadFile()[m
[32m+[m		[32m{[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                //m·ªü file, return khi file r·ªóng[m
[32m+[m[32m                FileStream fileStream = new FileStream(path, FileMode.OpenOrCreate);[m
[32m+[m[32m                FileInfo fileInfo = new FileInfo(path);[m
[32m+[m[32m                fileInfo.Attributes = FileAttributes.Hidden;[m
[32m+[m[32m                if (fileStream.Length == 0)[m
[32m+[m[32m                {[m
[32m+[m[32m                    Done = true;[m
[32m+[m[32m                    fileStream.Close();[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m[32m                //n·∫°p data th√¥ t·ª´ file[m[41m			[m
[32m+[m[32m                byte[] dataBytes = new byte[fileStream.Length];[m
[32m+[m[32m                fileStream.Read(dataBytes, 0, dataBytes.Length);[m
[32m+[m[32m                //gi·∫£i m√£[m[41m	[m
[32m+[m[32m                passwordBytes = password;[m
[32m+[m[32m                dataString = Encoding.Unicode.GetString(GCoding.Decoding(dataBytes, passwordBytes));[m
[32m+[m[32m                //ki·ªÉm tra ƒëo·∫°n passCode tr∆∞·ªõc chu·ªói v√† xo√° n√≥ ƒëi[m[41m	[m
[32m+[m[32m                if (dataString.Substring(0, passCode.Length) != passCode)[m
[32m+[m[32m                {[m
[32m+[m[32m                    password_is_wrong = true;[m
[32m+[m[32m                    Done = true;[m
[32m+[m[32m                    fileStream.Close();[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m[32m                dataString = dataString.Substring(passCode.Length);[m
[32m+[m[32m                //n·∫°p treeView[m
[32m+[m[32m                foreach (TreeNode node in treeView.Nodes)[m
[32m+[m[32m                {[m
[32m+[m[32m                    int index = -leftCode.Length;[m
[32m+[m[32m                    while (true)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        index += leftCode.Length;[m
[32m+[m[32m                        if (index == dataString.Length - 1 || dataString.Substring(index).IndexOf(rightCode) == 0) break;[m
[32m+[m[32m                        index = AddTreeNode(node, index);//ƒë·ªá quy[m
[32m+[m[32m                    }[m
[32m+[m[32m                    node.Expand();[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m[32m                //d·ªçn d·∫πp[m
[32m+[m[32m                fileStream.Close();[m
[32m+[m[32m                //show hardware[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (run_ShowComputer) info = ShowComputer.Show();[m
[32m+[m[32m                }[m
[32m+[m[32m                catch (Exception e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    MessageBox.Show(e.ToString(), "ERROR", MessageBoxButtons.OK, MessageBoxIcon.Error);[m
[32m+[m[32m                }[m
[32m+[m[32m                Done = true;[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception e)[m
[32m+[m[32m            {[m
[32m+[m[32m                MessageBox.Show(e.ToString(), "ERROR", MessageBoxButtons.OK, MessageBoxIcon.Error);[m
[32m+[m[32m                Application.Exit();[m
[32m+[m[32m            }[m
[32m+[m		[32m}[m
[32m+[m		[32mprivate int AddTreeNode(TreeNode treeRoot, int indexCurrent)[m
[32m+[m		[32m{[m
[32m+[m			[32mint index = dataString.Substring(indexCurrent).IndexOf(leftCode) + indexCurrent;[m[41m			[m
[32m+[m			[32m//t·∫°o m·ªõi m·ªôt TreeNode[m
[32m+[m			[32mTreeNode treeNode = new TreeNode();[m
[32m+[m			[32mstring dataString2 = dataString.Substring(indexCurrent, index - indexCurrent);//ƒëo·∫°n string ch·ª©a d·ªØ li·ªáu c·ªßa m·ªôt TreeNode[m
[32m+[m			[32mindexCurrent = dataString2.IndexOf(nameCode);[m
[32m+[m			[32mtreeNode.Text = dataString2.Substring(0, indexCurrent);//ƒëo·∫°n ƒë·∫ßu c·ªßa dataString2 ch·ª©a t√™n c·ªßa TreeNode[m
[32m+[m			[32mtreeNode.Name = dataString2.Substring(indexCurrent + nameCode.Length);//ƒëo·∫°n c√≤n l·∫°i c·ªßa dataString2 ch·ª©a n·ªôi dung c·ªßa TreeNode[m
[32m+[m[32m            //n·∫øu ƒë√¢y l√† m·ªôt LockedTreeNode[m
[32m+[m[32m            if (treeNode.Name.Contains(lockCode))[m
[32m+[m[32m            {[m
[32m+[m[32m                string fileName = treeNode.Name.Substring(0, treeNode.Name.IndexOf(lockCode));[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    //t·∫°o th∆∞ m·ª•c ƒë·ªÉ l∆∞u file (n·∫øu ch∆∞a c√≥)[m
[32m+[m[32m                    string path = Application.StartupPath + "\\Data\\";[m
[32m+[m[32m                    if (!Directory.Exists(path))[m
[32m+[m[32m                        Directory.CreateDirectory(path);[m
[32m+[m[32m                    treeNode.Tag = new DataBytes(File.ReadAllBytes(path + fileName));[m
[32m+[m[32m                }[m
[32m+[m[32m                catch (Exception e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    MessageBox.Show("Can't write the file: " + fileName + "\n" + e.ToString(), "ERROR", MessageBoxButtons.OKCancel, MessageBoxIcon.Error);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m			[32mtreeRoot.Nodes.Add(treeNode);[m
[32m+[m			[32m//b·∫Øt ƒë·∫ßu ƒë·ªá quy[m
[32m+[m			[32mwhile (true)[m
[32m+[m			[32m{[m
[32m+[m				[32mindex += leftCode.Length;[m
[32m+[m				[32mif (index == dataString.Length - 1 || dataString.Substring(index).IndexOf(rightCode) == 0) return index;[m
[32m+[m				[32mindex = AddTreeNode(treeNode, index);[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        //l∆∞u d·ªØ li·ªáu[m
[32m+[m[32m        #region SaveFile[m[41m	[m
[32m+[m[32m        private void SaveFile()[m
[32m+[m		[32m{[m
[32m+[m[41m        [m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                //l∆∞u data[m
[32m+[m[32m                dataString = savingString;[m
[32m+[m[32m                FileInfo fileInfo = new FileInfo(path);[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    fileInfo.Attributes = FileAttributes.Normal;[m
[32m+[m[32m                }[m
[32m+[m[32m                catch//tr∆∞·ªùng h·ª£p n·∫øu file kh√¥ng t·ªìn t·∫°i[m
[32m+[m[32m                {[m
[32m+[m[32m                    using (File.Create(path))[m
[32m+[m[32m                        fileInfo = new FileInfo(path);[m
[32m+[m[32m                    fileInfo.Attributes = FileAttributes.Normal;[m
[32m+[m[32m                }[m
[32m+[m[32m                dataString = passCode + dataString;//ƒëo·∫°n m√£ d√πng ƒë·ªÉ ki·ªÉm tra password l√∫c m·ªü ch∆∞∆°ng tr√¨nh[m
[32m+[m[32m                File.WriteAllBytes(path, GCoding.Encoding(Encoding.Unicode.GetBytes(dataString.ToCharArray()), password));[m
[32m+[m[32m                fileInfo.Attributes = FileAttributes.Hidden;[m
[32m+[m[32m                Done = true;[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception e)[m
[32m+[m[32m            {[m
[32m+[m[32m                MessageBox.Show(e.ToString(), "ERROR", MessageBoxButtons.OK, MessageBoxIcon.Error);[m
[32m+[m[32m                Application.Exit();[m
[32m+[m[32m            }[m
[32m+[m		[32m}[m
[32m+[m[32m        /*[m
[32m+[m		[32mprivate void TreeNodeToString(TreeNode treeRoot)[m
[32m+[m		[32m{[m
[32m+[m			[32mif (treeRoot.Nodes.Count == 0) return;[m
[32m+[m			[32mforeach (TreeNode nodes in treeRoot.Nodes)[m
[32m+[m			[32m{[m
[32m+[m[32m                //n·∫øu l√† UnlockedTreeNode[m
[32m+[m[32m                if (nodes.Name.Contains(lockCode) && nodes.Name.Substring(0, nodes.Name.IndexOf(lockCode)).Contains(nodesCode))[m
[32m+[m[32m                    SaveUnlockedTreeNode(nodes);[m
[32m+[m[32m                //n·∫øu l√† LockedTreeNode[m
[32m+[m[32m                else if (nodes.Name.Contains(lockCode))[m
[32m+[m[41m                    [m
[32m+[m[32m                savingString += nodes.Text + nameCode + nodes.Name + leftCode;[m
[32m+[m[32m                TreeNodeToString(nodes);[m
[32m+[m[32m                savingString += rightCode;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m[32m        */[m
[32m+[m[32m        private string TreeNodeToString(TreeNode node, bool is_get_text_nodes)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (node.Nodes == null)[m
[32m+[m[32m                return "";[m
[32m+[m[32m            string text_nodes_ = "";[m
[32m+[m[32m            foreach (TreeNode a_node in node.Nodes)// nodes = A[m
[32m+[m[32m            {[m
[32m+[m[32m                //n·∫øu l√† m·ªôt UnlockedTreeNode[m
[32m+[m[32m                if (a_node.Name.Contains(nodesCode))[m
[32m+[m[32m                {[m
[32m+[m[32m                    /*[m
[32m+[m[32m                        Tr·∫°ng th√°i c·ªßa Unlocked: TreeNode.Name = text-content + nodesCode + text-nodes + pathCode + path + lockCode + password (ASCII)[m
[32m+[m[32m                        M·ªôt TreeNode file s·∫Ω l∆∞u: passCode + text-content + nodesCode + text-nodes[m
[32m+[m[32m                    */[m
[32m+[m[32m                    byte[] password = Encoding.ASCII.GetBytes(a_node.Name.Substring(a_node.Name.LastIndexOf(lockCode) + lockCode.Length).ToCharArray());[m
[32m+[m[32m                    string text_content = a_node.Name.Substring(0, a_node.Name.IndexOf(nodesCode));[m
[32m+[m[32m                    //ƒë∆∞a TreeNode v·ªÅ tr·∫°ng th√°i Locked (TreeNode.Name = path + lockCode)[m
[32m+[m[32m                    a_node.Name = a_node.Name.Substring(a_node.Name.LastIndexOf(pathCode) + pathCode.Length, a_node.Name.LastIndexOf(lockCode) + lockCode.Length - a_node.Name.LastIndexOf(pathCode) - pathCode.Length);[m
[32m+[m[32m                    //b·∫Øt ƒë·∫ßu ti·ªÅn h√†nh c√°c b∆∞·ªõc ƒë·ªÉ l∆∞u d·ªØ li·ªáu v√† lock TreeNode l·∫°i[m
[32m+[m[32m                    //t·∫°o th∆∞ m·ª•c ƒë·ªÉ l∆∞u file (n·∫øu ch∆∞a c√≥)[m
[32m+[m[32m                    string path = Application.StartupPath + "\\Data\\";[m
[32m+[m[32m                    if (!Directory.Exists(path))[m
[32m+[m[32m                        Directory.CreateDirectory(path);[m
[32m+[m[32m                    //l·∫•y ƒë∆∞·ªùng d·∫´n ƒë·ªÉ l∆∞u TreeNode[m
[32m+[m[32m                    string fileName = a_node.Name.Substring(0, a_node.Name.LastIndexOf(lockCode));[m
[32m+[m[32m                    //ƒë·ªá quy, chuy·ªÉn c√°c TreeNodes con c·ªßa TreeNode ƒë√≥ th√†nh chu·ªói[m[41m		[m
[32m+[m[32m                    string text_nodes = text_nodes = TreeNodeToString(a_node, true) + rightCode;[m
[32m+[m[32m                    //xo√° c√°c TreeNodes con[m
[32m+[m[32m                    a_node.Nodes.Clear();[m
[32m+[m[32m                    //t·∫°o bytes d·ªØ li·ªáu coded_text[m
[32m+[m[32m                    byte[] bytes = GCoding.Encoding(Encoding.Unicode.GetBytes((passCode + text_content + nodesCode + text_nodes).ToCharArray()), password);[m
[32m+[m[32m                    a_node.Tag = new DataBytes(bytes);[m
[32m+[m[32m                    //ti·∫øn h√†nh l∆∞u[m
[32m+[m[32m                    try[m
[32m+[m[32m                    {[m
[32m+[m[32m                        File.WriteAllBytes(path + fileName, bytes);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    catch (Exception E)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        notifyIcon.ShowBalloonTip(1000, "Can't write the file: " + fileName, E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                //n·∫øu l√† m·ªôt TreeNode b√¨nh th∆∞·ªùng[m
[32m+[m[32m                if (is_get_text_nodes)[m
[32m+[m[32m                    text_nodes_ += a_node.Text + nameCode + a_node.Name + leftCode + TreeNodeToString(a_node, true) + rightCode;[m
[32m+[m[32m                else[m
[32m+[m[32m                {[m
[32m+[m[32m                    savingString += a_node.Text + nameCode + a_node.Name + leftCode;[m
[32m+[m[32m                    TreeNodeToString(a_node, false);[m
[32m+[m[32m                    savingString += rightCode;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            return text_nodes_;[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        #region timers[m
[32m+[m[32m        private void timer1_Tick(object sender, EventArgs e)[m
[32m+[m		[32m{[m
[32m+[m[41m			[m
[32m+[m			[32mif(U < 3)[m
[32m+[m			[32m{[m
[32m+[m				[32mU++;[m
[32m+[m				[32mlabel1.Text += '.';[m
[32m+[m[32m                Text += '.';[m
[32m+[m[32m            }[m
[32m+[m			[32melse[m
[32m+[m			[32m{[m
[32m+[m				[32mU = 0;[m
[32m+[m				[32mlabel1.Text = "Loading";[m
[32m+[m[32m                Text = "Loading";[m
[32m+[m[32m            }[m
[32m+[m		[32m}[m
[32m+[m		[32mprivate void timer2_Tick(object sender, EventArgs e)[m
[32m+[m		[32m{[m
[32m+[m			[32mif (Done) Close();[m
[32m+[m		[32m}[m
[32m+[m[32m        #endregion[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
