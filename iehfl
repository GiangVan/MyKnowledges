[32m+[m[32m        private const string leftCode = "!2||-@#(";[m
[32m+[m[32m        private const string rightCode = ")#@-||2!";[m
[32m+[m[32m        private const string nameCode = "!2||-@#x";//d√πng ƒë·ªÉ nh·∫≠n bi·∫øt TreeRoot[m
