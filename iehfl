[32m+[m[32m            labelPosition.Text = "Location: null";[m
[32m+[m[32m            //tr·∫£ v·ªÅ ƒë·ªô d√†i chu·ªói t√¨m ki·∫øm[m
[32m+[m[32m            labelLength.Text = "Length: " + textBoxFind.TextLength;[m
[32m+[m[32m            if (textBoxFind.TextLength == 0) return;[m
[32m+[m[32m            //tr·∫£ v·ªÅ s·ªë l∆∞·ª£ng ph·∫ßn t·ª≠ ƒë√£ t√¨m ƒë∆∞·ª£c[m
[32m+[m[32m            number_of_units_found = (textBoxContent.TextLength - textBoxContent.Text.Replace(textBoxFind.Text, "").Length) / textBoxFind.TextLength;[m
[32m+[m[32m            labelFound.Text = "Had found: " + number_of_units_found;[m
[32m+[m[32m            if (number_of_units_found == 0) return;[m[41m            [m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        private void buttonFind_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (number_of_units_found == 0 || textBoxFind.TextLength == 0)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormNotification("Not found!", true);[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m[41m [m
[32m+[m[32m            //find[m
[32m+[m[32m            int index = new int();[m
[32m+[m[32m            if (position_of_the_string_has_been_found != 0) index = position_of_the_string_has_been_found + textBoxFind.TextLength;[m
[32m+[m[32m            position_of_the_string_has_been_found = textBoxContent.Text.Substring(index).IndexOf(textBoxFind.Text) + index;[m
[32m+[m[32m            if(position_of_the_string_has_been_found == index - 1)//n·∫øu t√¨m ƒë·∫øn th·∫±ng cu·ªëi c√πng th√¨ quay l·∫°i th·∫±ng ƒë·∫ßu ti√™n[m
[32m+[m[32m                position_of_the_string_has_been_found = textBoxContent.Text.IndexOf(textBoxFind.Text);[m
[32m+[m[32m            labelPosition.Text = "Location: " + position_of_the_string_has_been_found;[m
[32m+[m[32m            //b√¥i ƒëen d·ªØ li·ªáu ƒë√£ t√¨m th·∫•y[m
[32m+[m[32m            FormMain.Activate();[m
[32m+[m[32m            textBoxContent.Select();[m
[32m+[m[32m            textBoxContent.SelectionStart = position_of_the_string_has_been_found;[m
[32m+[m[32m            textBoxContent.SelectionLength = textBoxFind.TextLength;[m
[32m+[m[32m            textBoxContent.ScrollToCaret();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void FormFindAndReplace_FormClosing(object sender, FormClosingEventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            Enabled = false;[m
[32m+[m[32m            Hide();[m
[32m+[m[32m            e.Cancel = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public void SetEnabledForButtonReplace(bool value)[m
[32m+[m[32m        {[m
[32m+[m[32m            buttonReplace.Enabled = value;[m
[32m+[m[32m            buttonReplaceAll.Enabled = value;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void buttonReplace_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (number_of_units_found == 0 || textBoxFind.TextLength == 0)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormNotification("Not found!", true);[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            //find[m
[32m+[m[32m            int index = position_of_the_string_has_been_found;[m
[32m+[m[32m            if (position_of_the_string_has_been_found == 0) index = textBoxContent.Text.IndexOf(textBoxFind.Text);[m
[32m+[m[32m            //b√¥i ƒëen d·ªØ li·ªáu ƒë√£ t√¨m th·∫•y[m
[32m+[m[32m            FormMain.Activate();[m
[32m+[m[32m            textBoxContent.Select();[m
[32m+[m[32m            textBoxContent.SelectionStart = index;[m
[32m+[m[32m            textBoxContent.SelectionLength = textBoxFind.TextLength;[m
[32m+[m[32m            //replace[m
[32m+[m[32m            string str = Clipboard.GetText();[m
[32m+[m[32m            Clipboard.SetText(textBoxReplace.Text);[m
[32m+[m[32m            SendKeys.SendWait("^V");[m
[32m+[m[32m            Clipboard.SetText(str);[m
[32m+[m[32m            //find next[m
[32m+[m[32m            position_of_the_string_has_been_found = index + textBoxReplace.TextLength + textBoxContent.Text.Substring(textBoxContent.SelectionStart).IndexOf(textBoxFind.Text);[m
[32m+[m[32m            if (position_of_the_string_has_been_found == index + textBoxReplace.TextLength - 1) position_of_the_string_has_been_found = 0;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void buttonReplaceAll_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (number_of_units_found == 0 || textBoxFind.TextLength == 0)[m
[32m+[m[32m            {[m
[32m+[m[32m                new FormNotification("Not found!", true);[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            if(MessageBox.Show("Are you sure?", "", MessageBoxButtons.YesNoCancel, MessageBoxIcon.Question) == DialogResult.Yes)[m
[32m+[m[32m            {[m
[32m+[m[32m                textBoxContent.Text = textBoxContent.Text.Replace(textBoxFind.Text, textBoxReplace.Text);[m
[32m+[m[32m                FormMain.Activate();[m
[32m+[m[32m                textBoxContent.Select();[m
[32m+[m[32m                textBoxContent.SelectionLength = 0;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
