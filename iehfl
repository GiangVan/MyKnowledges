[32m+[m[32m                        FolderBrowserDialog openFileDialog = new FolderBrowserDialog();[m
[32m+[m[32m                        if (openFileDialog.ShowDialog() == DialogResult.OK && openFileDialog.SelectedPath.Length > 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            SetClipboardAndPaste(openFileDialog.SelectedPath);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                #endregion[m
