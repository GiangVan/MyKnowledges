[32m+[m[32m            //AutoSave[m
[32m+[m[32m            #region[m
[32m+[m[32m                //checkBoxAutoSave_CheckedChanged[m
[32m+[m[32m                private void checkBoxAutoSave_CheckedChanged(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            timerAutoSave.Enabled = checkBoxAutoSave.Checked;[m
[32m+[m[32m                            //m·ªü[m
[32m+[m[32m                            if (checkBoxAutoSave.Checked)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                if (checkBox_TurboBoost.Checked)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    labelAutoSave.Top = 0;[m
[32m+[m[32m                                    panel_trackBarAutoSave.Top = 14;[m
[32m+[m[32m                                }[m
[32m+[m[32m                                else[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    new AnimationMOVE(labelAutoSave, 0, false, true, 0, 17);[m
[32m+[m[32m                                    new AnimationMOVE(panel_trackBarAutoSave, 14, false, true, 90, 17);[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                            //ƒë√≥ng[m
[32m+[m[32m                            else[m
[32m+[m[32m                            {[m
[32m+[m[32m                                if (checkBox_TurboBoost.Checked)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    labelAutoSave.Top = 58;[m
[32m+[m[32m                                    panel_trackBarAutoSave.Top = 73;[m
[32m+[m[32m                                }[m
[32m+[m[32m                                else[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    new AnimationMOVE(labelAutoSave, 58, true, true, 150, 19);[m
[32m+[m[32m                                    new AnimationMOVE(panel_trackBarAutoSave, 73, true, true, 0, 19);[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                            //save file[m
[32m+[m[32m                            string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\AutoSave";[m
[32m+[m[32m                            if (!Directory.Exists(path)) Directory.CreateDirectory(path);[m
[32m+[m[32m                            try[m
[32m+[m[32m                            {[m
[32m+[m[32m                                File.WriteAllText(path + "\\AutoSave.txt", checkBoxAutoSave.Checked.ToString());[m
[32m+[m[32m                            }[m
[32m+[m[32m                            catch (Exception E)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                notifyIcon.ShowBalloonTip(1000, "Writing AutoSave.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                //trackBarAutoSave_Scroll[m
[32m+[m[32m                private void trackBarAutoSave_Scroll(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            labelAutoSave.Text = "Saving per " + trackBarAutoSave.Value + " minutes";[m
[32m+[m[32m                            timerAutoSave.Interval = trackBarAutoSave.Value * 60000;[m
[32m+[m[32m                            //save file[m
[32m+[m[32m                            string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\AutoSave";[m
[32m+[m[32m                            if (!Directory.Exists(path)) Directory.CreateDirectory(path);[m
[32m+[m[32m                            try[m
[32m+[m[32m                            {[m
[32m+[m[32m                                File.WriteAllText(path + "\\Value.txt", trackBarAutoSave.Value.ToString());[m
[32m+[m[32m                            }[m
[32m+[m[32m                            catch (Exception E)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                notifyIcon.ShowBalloonTip(1000, "Writing Value.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
