[32m+[m[32m                }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //cut rows[m
[32m+[m[32m                #region[m
[32m+[m[32m                    private void cutRowsToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        CutRows();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void CutRows()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        SeleteRows();[m
[32m+[m[32m                        SendKeys.Send("^X");[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void SeleteRows()[m
[32m+[m[32m                    {[m
[32m+[m[32m                        int indexStart;[m
[32m+[m[32m                        if(textBoxContent.SelectedText.Length > 2) indexStart = textBoxContent.Text.Substring(0, textBoxContent.SelectionStart + 2).LastIndexOf("\r\n");[m
[32m+[m[32m                        else indexStart = textBoxContent.Text.Substring(0, textBoxContent.SelectionStart).LastIndexOf("\r\n");[m
[32m+[m[32m                        int index = textBoxContent.SelectionStart + textBoxContent.SelectionLength;[m
[32m+[m[32m                        int indexEnd = textBoxContent.Text.Substring(index).IndexOf("\r\n");[m
[32m+[m[32m                        if (indexStart == -1) indexStart = 0;[m
[32m+[m[32m                        if (indexStart == 0) indexEnd += 2;[m
[32m+[m[32m                        if (indexEnd == -1) indexEnd = textBoxContent.Text.Length;[m
[32m+[m[32m                        else indexEnd += index;[m
[32m+[m[32m                        textBoxContent.SelectionStart = indexStart;[m
[32m+[m[32m                        textBoxContent.SelectionLength = indexEnd - indexStart;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    //b√¥i ƒëen v√πng chu·∫©n b·ªã cut khi r√™ chu·ªôt qua cutRowsToolStripMenuItem[m
[32m+[m[32m                    private void cutRowsToolStripMenuItem_MouseEnter(object sender, EventArgs e)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        index_text = textBoxContent.SelectionStart;[m
[32m+[m[32m                        length_text = textBoxContent.SelectionLength;[m
[32m+[m[32m                        SeleteRows();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    private void cutRowsToolStripMenuItem_MouseLeave(object sender, EventArgs e)[m
[32m+[m[32m                {[m
[32m+[m[32m                    textBoxContent.SelectionStart = index_text;[m
[32m+[m[32m                    textBoxContent.SelectionLength = length_text;[m
[32m+[m[32m                }[m
[32m+[m[32m                #endregion[m
[32m+[m
[32m+[m[32m                //word wrap[m
[32m+[m[32m                #region[m
[32m+[m[32m                    private void wordWrapToolStripMenuItem_Click(object sender, EventArgs e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (textBoxContent.WordWrap)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                wordWrapToolStripMenuItem.Image = Properties.Resources._checked;[m
[32m+[m[32m                                textBoxContent.WordWrap = false;[m
[32m+[m[32m                            }[m
[32m+[m[32m                            else[m
[32m+[m[32m                            {[m
[32m+[m[32m                                wordWrapToolStripMenuItem.Image = null;[m
[32m+[m[32m                                textBoxContent.WordWrap = true;[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                #endregion[m
[32m+[m
