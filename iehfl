[32m+[m[32m                    fileName = "AutoSave.txt";[m
[32m+[m[32m                    string fileName2 = "Value.txt";[m
[32m+[m[32m                    if (File.Exists(pathAutoSave + fileName))[m
[32m+[m[32m                    {[m
[32m+[m[32m                        bool bo = checkBox_TurboBoost.Checked;[m
[32m+[m[32m                        try[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (File.ReadAllText(pathAutoSave + fileName).Length > 0)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                checkBox_TurboBoost.Checked = true;//ƒë·∫∑t true ƒë·ªÉ ngƒÉn AnimationMOVE ch·∫°y[m
[32m+[m[32m                                checkBoxAutoSave.Checked = bool.Parse(File.ReadAllText(pathAutoSave + fileName));[m
[32m+[m[32m                                checkBox_TurboBoost.Checked = bo;//ƒë·∫∑t l·∫°i nh∆∞ c≈©[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        catch (Exception e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            notifyIcon.ShowBalloonTip(1000, "Reading " + fileName + " file failed!", e.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    if (checkBoxAutoSave.Checked)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        labelAutoSave.Top = 0;[m
[32m+[m[32m                        panel_trackBarAutoSave.Top = 14;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else[m
[32m+[m[32m                    {[m
[32m+[m[32m                        labelAutoSave.Top = 58;[m
[32m+[m[32m                        panel_trackBarAutoSave.Top = 73;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    if (File.Exists(pathAutoSave + fileName2))[m
[32m+[m[32m                    {[m
[32m+[m[32m                        try[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (File.ReadAllText(pathAutoSave + fileName2).Length > 0)[m
[32m+[m[32m                            {[m
[32m+[m[32m                                int value = int.Parse(File.ReadAllText(pathAutoSave + fileName2));[m
[32m+[m[32m                                if(value >= 5 && value <= 180)[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    trackBarAutoSave.Value = value;[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                        catch (Exception e)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            notifyIcon.ShowBalloonTip(1000, "Reading " + fileName2 + " file failed!", e.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    labelAutoSave.Text = "Saving per " + trackBarAutoSave.Value + " minutes";[m
[32m+[m[32m                    timerAutoSave.Interval = trackBarAutoSave.Value * 60000;[m
[32m+[m[32m                #endregion[m
[32m+[m[32m                //checkBox_StartWithWindows[m
[32m+[m[32m                #region[m
