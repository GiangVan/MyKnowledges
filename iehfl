[32m+[m[32m                        timer.Enabled = false;[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    string path = Environment.GetFolderPath(Environment.SpecialFolder.MyDocuments) + "\\My Knowledge\\ShowTime.txt";[m
[32m+[m[32m                    try[m
[32m+[m[32m                    {[m
[32m+[m[32m                        File.WriteAllText(path, checkBoxShowTime.Checked.ToString());[m
[32m+[m[32m                    }[m
[32m+[m[32m                    catch (Exception E)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        notifyIcon.ShowBalloonTip(1000, "Writing ShowTime.txt file failed!", E.ToString(), ToolTipIcon.Warning);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
